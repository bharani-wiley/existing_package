FORM READ_SINGLE_ZQTCV_EXCL_RESN.
  SELECT SINGLE * FROM ZQTC_EXCL_RESN WHERE
EXCL_RESN = ZQTCV_EXCL_RESN-EXCL_RESN .
ZQTCV_EXCL_RESN-MANDT =
ZQTC_EXCL_RESN-MANDT .
ZQTCV_EXCL_RESN-EXCL_RESN =
ZQTC_EXCL_RESN-EXCL_RESN .
    SELECT SINGLE * FROM ZQTCT_EXCL_RESN WHERE
EXCL_RESN = ZQTC_EXCL_RESN-EXCL_RESN AND
SPRAS = SY-LANGU .
    IF SY-SUBRC EQ 0.
ZQTCV_EXCL_RESN-EXCL_RESN_D =
ZQTCT_EXCL_RESN-EXCL_RESN_D .
    ELSE.
      CLEAR SY-SUBRC.
      CLEAR ZQTCV_EXCL_RESN-EXCL_RESN_D .
    ENDIF.
ENDFORM.
