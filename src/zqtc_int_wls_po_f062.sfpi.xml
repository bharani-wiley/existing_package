      <FPCLINE>*----------------------------------------------------------------------*</FPCLINE>
      <FPCLINE>* INTERFACE NAME:ZQTC_INT_WLS_PO_F062</FPCLINE>
      <FPCLINE>* INTERFACE DESCRIPTION: Purchase order form for WLS</FPCLINE>
      <FPCLINE>* DEVELOPER:MIMMADISET</FPCLINE>
      <FPCLINE>* CREATION DATE:06/16/2020</FPCLINE>
      <FPCLINE>* OBJECT ID:F062</FPCLINE>
      <FPCLINE>* TRANSPORT NUMBER(S):ED2K918463</FPCLINE>
      <FPCLINE>*----------------------------------------------------------------------*</FPCLINE>
      <FPCLINE>TYPES:BEGIN OF ty_header,</FPCLINE>
      <FPCLINE>        supp_name TYPE name1_gp,  &quot;Supplier Contact</FPCLINE>
      <FPCLINE>        pay_text  TYPE text1_052,  &quot;Payment Terms</FPCLINE>
      <FPCLINE>        EMAIL_INV TYPE AD_SMTPADR,</FPCLINE>
      <FPCLINE>        ername    TYPE char80,</FPCLINE>
      <FPCLINE>        VEN_adrnr TYPE adrnr,              &quot;Vendor Address</FPCLINE>
      <FPCLINE>        ship_adrnr type adrnr,</FPCLINE>
      <FPCLINE>        bedat type char30,</FPCLINE>
      <FPCLINE>        test_ref_doc type char50,</FPCLINE>
      <FPCLINE>      END OF ty_header,</FPCLINE>
      <FPCLINE>      BEGIN OF ty_final,</FPCLINE>
      <FPCLINE>        ebelp   TYPE  ebelp,          &quot;Item Number of Purchasing Document</FPCLINE>
      <FPCLINE>        old_mat TYPE  bismt,          &quot;Old material number</FPCLINE>
      <FPCLINE>        eindt   TYPE  eindt,          &quot;Item Delivery Date</FPCLINE>
      <FPCLINE>        matnr   TYPE  matnr,          &quot;Material Number</FPCLINE>
      <FPCLINE>        menge   TYPE  bstmg,          &quot;Purchase Order Quantity</FPCLINE>
      <FPCLINE>        meins   TYPE  MSEHT,          &quot;Purchase Order Unit of Measure</FPCLINE>
      <FPCLINE>        netpr   TYPE  bprei,          &quot;Net price</FPCLINE>
      <FPCLINE>        netwr   TYPE  bwert,          &quot;Net Order Value</FPCLINE>
      <FPCLINE>        mat_desc TYPE  txz01,         &quot;Material Description</FPCLINE>
      <FPCLINE>        it_text TYPE  char100,        &quot;Item text</FPCLINE>
      <FPCLINE>        po_text TYPE  char100,        &quot;PO text</FPCLINE>
      <FPCLINE>      END OF ty_final,</FPCLINE>
      <FPCLINE>*************Structure For T001 Table</FPCLINE>
      <FPCLINE>       BEGIN OF ty_t001,</FPCLINE>
      <FPCLINE>         bukrs TYPE bukrs,   &quot;Company Code</FPCLINE>
      <FPCLINE>         land1 TYPE land1,   &quot;Country Key</FPCLINE>
      <FPCLINE>         adrnr TYPE adrnr,   &quot;Address</FPCLINE>
      <FPCLINE>       END OF ty_t001,</FPCLINE>
      <FPCLINE>         BEGIN OF ty_ship,</FPCLINE>
      <FPCLINE>         ship1 type  TDLINE,</FPCLINE>
      <FPCLINE>         ship2 type  TDLINE,</FPCLINE>
      <FPCLINE>         ship3 type  TDLINE,</FPCLINE>
      <FPCLINE>         ship4 type  TDLINE,</FPCLINE>
      <FPCLINE>         ship5 type  TDLINE,</FPCLINE>
      <FPCLINE>         ship6 type  TDLINE,</FPCLINE>
      <FPCLINE>         ship7 type  TDLINE,</FPCLINE>
      <FPCLINE>         ship8 type  TDLINE,</FPCLINE>
      <FPCLINE>       END OF ty_sHIP.</FPCLINE>
      <FPCLINE/>
      <FPCLINE/>
      <FPCLINE>******Table type declaration</FPCLINE>
      <FPCLINE>TYPES: tt_final    TYPE STANDARD TABLE OF ty_final.</FPCLINE>
