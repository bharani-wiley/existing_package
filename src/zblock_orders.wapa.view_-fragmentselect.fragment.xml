<core:FragmentDefinition  xmlns="sap.m" xmlns:core="sap.ui.core" id="F1" controllerName="Block_Orders.Block_Orders.controller.Worklist"                                                                                                                        
	xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:d="sap.ui.core.LayoutData" xmlns:u="sap.ui.unified" xmlns:c="sap.ui.core">                                                                                                                         
	<Dialog id="dgsel2" resizable="true" draggable="true" contentWidth="650px">                                                                                                                                                                                   
		<content>                                                                                                                                                                                                                                                    
			<l:Splitter orientation="Horizontal">                                                                                                                                                                                                                       
				<f:SimpleForm id="SimpleFormAdd" editable="false" layout="ResponsiveGridLayout">                                                                                                                                                                           
					<f:content>                                                                                                                                                                                                                                               
						<VBox>                                                                                                                                                                                                                                                   
							<Label text="Customer No" width="75px"/>                                                                                                                                                                                                                
							<MultiInput id="customerInput" showSuggestion="false" showValueHelp="true" valueHelpRequest="onHelpRequestKunnr"                                                                                                                                        
								tokenChange="customerChange" suggestionItemSelected="onSuggestioncustomer" valueHelpOnly="true" width="200px" placeholder="Sold-TO"></MultiInput>                                                                                                      
						</VBox>                                                                                                                                                                                                                                                  
						<VBox>                                                                                                                                                                                                                                                   
							<Label text="Dunning Procedure"/>                                                                                                                                                                                                                       
							<MultiComboBox id="Dunning" items="{/DunningProcedureSet}" textAlign="Center" textDirection="RTL" width="200px"                                                                                                                                         
								selectionChange="tabSelected" selectionFinish="SelTabEH1">                                                                                                                                                                                             
								<core:Item key="{MAHNA}" text="{MAHNA} - {TEXTM}"/>                                                                                                                                                                                                    
							</MultiComboBox>                                                                                                                                                                                                                                        
						</VBox>                                                                                                                                                                                                                                                  
						<VBox>                                                                                                                                                                                                                                                   
							<Label text="Dunning Level "/>                                                                                                                                                                                                                          
							<MultiComboBox change="onSelectChange" id="DunInput" forceSelection="false" textAlign="Center" textDirection="RTL" width="200px">                                                                                                                       
								<core:Item key="1" text="1"/>                                                                                                                                                                                                                          
								<core:Item key="2" text="2"/>                                                                                                                                                                                                                          
								<core:Item key="3" text="3"/>                                                                                                                                                                                                                          
								<core:Item key="4" text="4"/>                                                                                                                                                                                                                          
								<core:Item key="5" text="5"/>                                                                                                                                                                                                                          
								<core:Item key=">5" text=">5"/>                                                                                                                                                                                                                        
								</MultiComboBox>                                                                                                                                                                                                                                       
							</VBox>                                                                                                                                                                                                                                                 
							<!--<VBox>-->                                                                                                                                                                                                                                           
							<!--	<Label text="Sales Group"/>-->                                                                                                                                                                                                                     
							<!--	<MultiComboBox id="salegrpinput" items="{/SalesGroupSet}" textAlign="Center" textDirection="RTL" width="190px" selectionChange="tabSelected"-->                                                                                                    
							<!--		selectionFinish="SelTabEH" visible="false">-->                                                                                                                                                                                                    
							<!--		<core:Item key="{VKGRP}" text="{VKGRP} - {BEZEI}"/>-->                                                                                                                                                                                            
							<!--	</MultiComboBox>-->                                                                                                                                                                                                                                
							<!--</VBox>-->                                                                                                                                                                                                                                          
							<VBox>                                                                                                                                                                                                                                                  
								<Label text="Material"/>                                                                                                                                                                                                                               
								<MultiInput id="productInput" showSuggestion="false" showValueHelp="true" valueHelpRequest="onHelpRequestPRD" width="200px"                                                                                                                            
									tokenChange="mediaChange" suggestionItemSelected="onSuggestionItemMedia" valueHelpOnly="true" placeholder="Material"></MultiInput>                                                                                                                    
							</VBox>                                                                                                                                                                                                                                                 
						</f:content>                                                                                                                                                                                                                                             
					</f:SimpleForm>                                                                                                                                                                                                                                           
					<f:SimpleForm id="SimpleFormAdd2" editable="true" layout="ResponsiveGridLayout">                                                                                                                                                                          
						<f:content>                                                                                                                                                                                                                                              
							                                                                                                                                                                                                                                                        
							<VBox>                                                                                                                                                                                                                                                  
								<Label text="Order Type"/>                                                                                                                                                                                                                             
								<MultiComboBox id="sotyp" items="{/OrderTypeSet}" textAlign="Center" textDirection="RTL" width="200px" selectionChange="tabSelected"                                                                                                                   
									selectionFinish="SelTabEH">                                                                                                                                                                                                                           
									<core:Item key="{Auart}" text="{Auart} - {Bezei}"/>                                                                                                                                                                                                   
								</MultiComboBox>                                                                                                                                                                                                                                       
							</VBox>                                                                                                                                                                                                                                                 
							<VBox>                                                                                                                                                                                                                                                  
								<Label text="Date From "/>                                                                                                                                                                                                                             
								<DatePicker id="IDatefrom" value="" width="200px" valueFormat="yyyy-MM-dd" displayFormat="long"/>                                                                                                                                                      
							</VBox>                                                                                                                                                                                                                                                 
							<VBox>                                                                                                                                                                                                                                                  
								<Label text="Date To "/>                                                                                                                                                                                                                               
								<DatePicker id="IDateto" value="" width="200px" valueFormat="yyyy-MM-dd" displayFormat="long"/>                                                                                                                                                        
							</VBox>                                                                                                                                                                                                                                                 
						</f:content>                                                                                                                                                                                                                                             
					</f:SimpleForm>                                                                                                                                                                                                                                           
				</l:Splitter>                                                                                                                                                                                                                                              
			</content>                                                                                                                                                                                                                                                  
			<beginButton>                                                                                                                                                                                                                                               
				<Button id="getID" icon="sap-icon://process" tooltip="Process" press="Get87" type="Emphasized" visible="true"/>                                                                                                                                            
			</beginButton>                                                                                                                                                                                                                                              
			<endButton text="close">                                                                                                                                                                                                                                    
				<Button id="Cancelid" icon="sap-icon://sys-cancel" press="Cancel87" type="Reject" tooltip="close"/>                                                                                                                                                        
			</endButton>                                                                                                                                                                                                                                                
		</Dialog>                                                                                                                                                                                                                                                    
	</core:FragmentDefinition>                                                                                                                                                                                                                                    