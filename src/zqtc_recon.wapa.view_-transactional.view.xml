<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:layout="sap.ui.layout"                                                                                                                                                      
	controllerName="recon.ZRECON_DASHBOARD.controller.transactional" xmlns:html="http://www.w3.org/1999/xhtml">                                                                                                                                                   
	<App>                                                                                                                                                                                                                                                         
		<pages>                                                                                                                                                                                                                                                      
			<Page title="{source} Report" showNavButton="true" navButtonPress="onPageBack">                                                                                                                                                                             
				<content>                                                                                                                                                                                                                                                  
					<ObjectHeader responsive="true" fullScreenOptimized="true" titleActive="true" title="{wricef}" intro="{description}"                                                                                                                                      
						backgroundDesign="Translucent" class="sapUiResponsivePadding--header">                                                                                                                                                                                   
						<ObjectAttribute title="Start Date" text="2021-07-22"/>                                                                                                                                                                                                  
						<ObjectAttribute title="End Date" text="2021-07-22"/>                                                                                                                                                                                                    
						<statuses>                                                                                                                                                                                                                                               
							<ObjectStatus title="Source" text="{source}" state="Error"/>                                                                                                                                                                                            
							<!--<ObjectStatus title="Target" text="{target}" state="Success"/>-->                                                                                                                                                                                   
							<!--	<ObjectStatus title="Start Date" text="2021-07-22"/>-->                                                                                                                                                                                            
							<!--	<ObjectStatus title="End Date" text="2021-07-22"/>-->                                                                                                                                                                                              
						</statuses>                                                                                                                                                                                                                                              
					</ObjectHeader>                                                                                                                                                                                                                                           
					<Table inset="false" items="{ path: 'ErrorData>/'}" growing="true" growingThreshold="5">                                                                                                                                                                  
						<headerToolbar>                                                                                                                                                                                                                                          
							<OverflowToolbar>                                                                                                                                                                                                                                       
								<content>                                                                                                                                                                                                                                              
									<DateRangeSelection width="20%"></DateRangeSelection>                                                                                                                                                                                                 
									<Button text="Search" type="Emphasized" class="sapUiTinyMarginBeginEnd"></Button>                                                                                                                                                                     
									<ToolbarSpacer></ToolbarSpacer>                                                                                                                                                                                                                       
								</content>                                                                                                                                                                                                                                             
							</OverflowToolbar>                                                                                                                                                                                                                                      
						</headerToolbar>                                                                                                                                                                                                                                         
						<columns>                                                                                                                                                                                                                                                
							<Column width="10em">                                                                                                                                                                                                                                   
								<Text text="Dev Id"/>                                                                                                                                                                                                                                  
							</Column>                                                                                                                                                                                                                                               
							<!--<Column width="15em">-->                                                                                                                                                                                                                            
							<!--	<Text text="CorelationID"/>-->                                                                                                                                                                                                                     
							<!--</Column>-->                                                                                                                                                                                                                                        
							<!--<Column>-->                                                                                                                                                                                                                                         
							<!--	<Text text="Source"/>-->                                                                                                                                                                                                                           
							<!--</Column>-->                                                                                                                                                                                                                                        
							<Column>                                                                                                                                                                                                                                                
								<Text text="Posted On"/>                                                                                                                                                                                                                               
							</Column>                                                                                                                                                                                                                                               
							<Column>                                                                                                                                                                                                                                                
								<Text text="Status Code"/>                                                                                                                                                                                                                             
							</Column>                                                                                                                                                                                                                                               
							<Column>                                                                                                                                                                                                                                                
								<Text text="Status Message"/>                                                                                                                                                                                                                          
							</Column>                                                                                                                                                                                                                                               
						</columns>                                                                                                                                                                                                                                               
						<items>                                                                                                                                                                                                                                                  
							<ColumnListItem>                                                                                                                                                                                                                                        
								<cells>                                                                                                                                                                                                                                                
									<ObjectStatus text="{ErrorData>devId}" state="Information" class="sapUiTinyMargin" inverted="true"/>                                                                                                                                                  
									<!--<ObjectStatus title="Source" text="{source}" class="sapUiTinyMargin textclr"/>-->                                                                                                                                                                 
									<!--<ObjectStatus title="Target" text="{target}" class="sapUiTinyMargin textclr"/>-->                                                                                                                                                                 
									<Text text="26/7/2021"></Text>                                                                                                                                                                                                                        
									<ObjectStatus text="Error" state="Error" class="sapUiTinyMargin"/>                                                                                                                                                                                    
									<Text text="{ErrorData>desc}"></Text>                                                                                                                                                                                                                 
								</cells>                                                                                                                                                                                                                                               
							</ColumnListItem>                                                                                                                                                                                                                                       
						</items>                                                                                                                                                                                                                                                 
					</Table>                                                                                                                                                                                                                                                  
				</content>                                                                                                                                                                                                                                                 
			</Page>                                                                                                                                                                                                                                                     
		</pages>                                                                                                                                                                                                                                                     
	</App>                                                                                                                                                                                                                                                        
</mvc:View>                                                                                                                                                                                                                                                    