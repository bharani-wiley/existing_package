   <FUNCTIONS>
    <item>
     <FUNCNAME>ZQTC_OUTPUT_SUPP_RETRIEVAL</FUNCNAME>
     <SHORT_TEXT>FM for E098-Output Supplement Retrieval</SHORT_TEXT>
     <IMPORT>
      <RSIMP>
       <PARAMETER>IM_INPUT</PARAMETER>
       <TYP>ZTQTC_SUPPLEMENT_RET_INPUT</TYP>
      </RSIMP>
      <RSIMP>
       <PARAMETER>IM_AUART</PARAMETER>
       <TYP>AUART</TYP>
      </RSIMP>
     </IMPORT>
     <EXPORT>
      <RSEXP>
       <PARAMETER>EX_OUTPUT</PARAMETER>
       <REFERENCE>X</REFERENCE>
       <TYP>ZTQTC_OUTPUT_SUPP_RETRIEVAL</TYP>
      </RSEXP>
      <RSEXP>
       <PARAMETER>EX_OUTPUT_ALL</PARAMETER>
       <REFERENCE>X</REFERENCE>
       <TYP>ZTQTC_OUTPUT_SUPP_RETRIEVAL</TYP>
      </RSEXP>
     </EXPORT>
     <DOCUMENTATION>
      <RSFDO>
       <PARAMETER>IM_INPUT</PARAMETER>
       <KIND>P</KIND>
       <STEXT>Product Number</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>IM_AUART</PARAMETER>
       <KIND>P</KIND>
       <STEXT>SD Document Category</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>EX_OUTPUT</PARAMETER>
       <KIND>P</KIND>
       <STEXT>Table Type for ZSTQTC_OUTPUT_SUPP_RETRIEVAL</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>EX_OUTPUT_ALL</PARAMETER>
       <KIND>P</KIND>
       <STEXT>Table Type for ZSTQTC_OUTPUT_SUPP_RETRIEVAL</STEXT>
      </RSFDO>
     </DOCUMENTATION>
    </item>
    <item>
     <FUNCNAME>ZQTC_OUTPUT_SUPP_SAVE_OR_SEND</FUNCNAME>
     <SHORT_TEXT>Send output supplement as email or save in application server</SHORT_TEXT>
     <IMPORT>
      <RSIMP>
       <PARAMETER>IM_SAVE_FILE</PARAMETER>
       <OPTIONAL>X</OPTIONAL>
       <REFERENCE>X</REFERENCE>
       <TYP>XFELD</TYP>
      </RSIMP>
      <RSIMP>
       <PARAMETER>IM_SEND_EMAIL</PARAMETER>
       <OPTIONAL>X</OPTIONAL>
       <REFERENCE>X</REFERENCE>
       <TYP>XFELD</TYP>
      </RSIMP>
      <RSIMP>
       <PARAMETER>IM_DOC_NUMBER</PARAMETER>
       <REFERENCE>X</REFERENCE>
       <TYP>SAEOBJID</TYP>
      </RSIMP>
      <RSIMP>
       <PARAMETER>IM_PDF_STREAM</PARAMETER>
       <REFERENCE>X</REFERENCE>
       <TYP>FPCONTENT</TYP>
      </RSIMP>
      <RSIMP>
       <PARAMETER>IM_ATTACHMENT_NAME</PARAMETER>
       <REFERENCE>X</REFERENCE>
       <TYP>TEXT60</TYP>
      </RSIMP>
      <RSIMP>
       <PARAMETER>IM_INVOICE</PARAMETER>
       <OPTIONAL>X</OPTIONAL>
       <REFERENCE>X</REFERENCE>
       <TYP>XFELD</TYP>
      </RSIMP>
      <RSIMP>
       <PARAMETER>IM_ORDER</PARAMETER>
       <OPTIONAL>X</OPTIONAL>
       <REFERENCE>X</REFERENCE>
       <TYP>XFELD</TYP>
      </RSIMP>
      <RSIMP>
       <PARAMETER>IM_ATTACH_DOC</PARAMETER>
       <OPTIONAL>X</OPTIONAL>
       <REFERENCE>X</REFERENCE>
       <TYP>XFELD</TYP>
      </RSIMP>
     </IMPORT>
     <EXPORT>
      <RSEXP>
       <PARAMETER>EX_EMAIL_DATA</PARAMETER>
       <TYP>SOLIX_TAB</TYP>
      </RSEXP>
     </EXPORT>
     <EXCEPTION>
      <RSEXC>
       <EXCEPTION>FILE_NOT_FOUND</EXCEPTION>
      </RSEXC>
     </EXCEPTION>
     <DOCUMENTATION>
      <RSFDO>
       <PARAMETER>IM_SAVE_FILE</PARAMETER>
       <KIND>P</KIND>
       <STEXT>Save in application server</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>IM_SEND_EMAIL</PARAMETER>
       <KIND>P</KIND>
       <STEXT>Send Email</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>IM_DOC_NUMBER</PARAMETER>
       <KIND>P</KIND>
       <STEXT>Order/Quotation/Invoice No.</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>IM_PDF_STREAM</PARAMETER>
       <KIND>P</KIND>
       <STEXT>PDF of type RAW1024</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>IM_ATTACHMENT_NAME</PARAMETER>
       <KIND>P</KIND>
       <STEXT>Attachment name</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>IM_INVOICE</PARAMETER>
       <KIND>P</KIND>
       <STEXT>Invoice Form</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>IM_ORDER</PARAMETER>
       <KIND>P</KIND>
       <STEXT>Order Form</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>IM_ATTACH_DOC</PARAMETER>
       <KIND>P</KIND>
       <STEXT>Attach document in INVOICE/ORDER</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>EX_EMAIL_DATA</PARAMETER>
       <KIND>P</KIND>
       <STEXT>GBT: SOLIX as Table Type</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>FILE_NOT_FOUND</PARAMETER>
       <KIND>X</KIND>
       <STEXT>File not found in application server</STEXT>
      </RSFDO>
     </DOCUMENTATION>
    </item>
