<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">                                                                                                                                                                                               
	<Dialog id="dlgProcessEH" title="Enter Order Reason" contentWidth="200px" contentHeight="30px" draggable="true">                                                                                                                                              
		<content>                                                                                                                                                                                                                                                    
			<Label id="lblProcessEH" text="Order Reason" textDirection="RTL" design="Bold"></Label>                                                                                                                                                                     
			<Input id="inpProcessEH" width="100px" 	fieldWidth="10px" showValueHelp="true" valueHelpRequest="onF4" showSuggestion="true" suggest="onDlgProcessEHSuggest"                                                                                                
				suggestionItems="{processEHModel>/}">                                                                                                                                                                                                                      
				<suggestionItems>                                                                                                                                                                                                                                          
					<core:Item id="coreItems" text="{processEHModel>Augru}" key="{processEHModel>Augru}"/>                                                                                                                                                                    
				</suggestionItems>                                                                                                                                                                                                                                         
			</Input>                                                                                                                                                                                                                                                    
		</content>                                                                                                                                                                                                                                                   
		<beginButton>                                                                                                                                                                                                                                                
			<Button id="btnBeginProcessEH" type="Emphasized" text="OK" press="onBeginBtnProcessEH"></Button>                                                                                                                                                            
		</beginButton>                                                                                                                                                                                                                                               
		<endButton>                                                                                                                                                                                                                                                  
			<Button id="btnEndProcessEH" type="Emphasized" text="Close" press="onEndBthProcessEH"></Button>                                                                                                                                                             
		</endButton>                                                                                                                                                                                                                                                 
	</Dialog>                                                                                                                                                                                                                                                     
</core:FragmentDefinition>                                                                                                                                                                                                                                     