<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc">                                                                                                                                           
	<Dialog title="Filters" contentWidth="100%">                                                                                                                                                                                                                  
		<!--<l:VerticalLayout width="100%" class="gridWrapper">-->                                                                                                                                                                                                   
		<l:Grid>                                                                                                                                                                                                                                                     
			<VBox class="sapUiTinyMargin">                                                                                                                                                                                                                              
				<Label text="Sales Model:"></Label>                                                                                                                                                                                                                        
				<MultiInput id="idMISalesOrder" name="MODEL" showValueHelp="true" suggestionItems="{f4HelpsModel>/SMODELSet}"                                                                                                                                              
					valueHelpRequest="handleSalesModel($event,'SMODELSet','MODEL','MDESCR')">                                                                                                                                                                                 
					<core:Item key="{f4HelpsModel>MODEL}" text="{f4HelpsModel>MDESCR}"/>                                                                                                                                                                                      
					<layoutData>                                                                                                                                                                                                                                              
						<l:GridData spanXL="3" spanL="3" spanM="6" spanS="12"/>                                                                                                                                                                                                  
					</layoutData>                                                                                                                                                                                                                                             
				</MultiInput>                                                                                                                                                                                                                                              
			</VBox>                                                                                                                                                                                                                                                     
			<VBox class="sapUiTinyMargin">                                                                                                                                                                                                                              
				<Label text="Created Date:"></Label>                                                                                                                                                                                                                       
				<!--minDate="{path: 'f4HelpsModel>/ZCACONSTSet/0/Low',formatter: '.formatter.minDate'}"-->                                                                                                                                                                 
				<DateRangeSelection id="iddRCreatedDate" name="ERDAT" delimiter="," displayFormat="dd-MM-yyyy" change="onCreatedDateChange">                                                                                                                               
					<layoutData>                                                                                                                                                                                                                                              
						<l:GridData spanXL="3" spanL="3" spanM="6" spanS="12"/>                                                                                                                                                                                                  
					</layoutData>                                                                                                                                                                                                                                             
				</DateRangeSelection>                                                                                                                                                                                                                                      
			</VBox>                                                                                                                                                                                                                                                     
			<VBox class="sapUiTinyMargin">                                                                                                                                                                                                                              
				<Label text="Billing Date:"></Label>                                                                                                                                                                                                                       
				<DateRangeSelection id="iddRBillingDate" name="FKDAT" delimiter="," displayFormat="dd-MM-yyyy">                                                                                                                                                            
					<layoutData>                                                                                                                                                                                                                                              
						<l:GridData spanXL="3" spanL="3" spanM="6" spanS="12"/>                                                                                                                                                                                                  
					</layoutData>                                                                                                                                                                                                                                             
				</DateRangeSelection>                                                                                                                                                                                                                                      
			</VBox>                                                                                                                                                                                                                                                     
			<VBox class="sapUiTinyMargin">                                                                                                                                                                                                                              
				<Label text="Sales Org:"></Label>                                                                                                                                                                                                                          
				<MultiInput width="100%" id="idMISalesOrg" showValueHelp="true" name="VKORG" suggestionItems="{f4HelpsModel>/SalesOrgSet}"                                                                                                                                 
					valueHelpRequest="handleSalesModel($event,'SalesOrgSet','VKORG','VTEXT')">                                                                                                                                                                                
					<core:Item key="{f4HelpsModel>VKORG}" text="{f4HelpsModel>VTEXT}"/>                                                                                                                                                                                       
					<layoutData>                                                                                                                                                                                                                                              
						<l:GridData spanXL="3" spanL="3" spanM="6" spanS="12"/>                                                                                                                                                                                                  
					</layoutData>                                                                                                                                                                                                                                             
				</MultiInput>                                                                                                                                                                                                                                              
			</VBox>                                                                                                                                                                                                                                                     
			<VBox class="sapUiTinyMargin">                                                                                                                                                                                                                              
				<Label text="Order Type:"></Label>                                                                                                                                                                                                                         
				<MultiInput width="100%" id="idMIOrderType" name="AUART" showValueHelp="true" suggestionItems="{f4HelpsModel>/OrderTypeSet}"                                                                                                                               
					valueHelpRequest="handleSalesModel($event,'OrderTypeSet','AUART','BEZEI')">                                                                                                                                                                               
					<core:Item key="{f4HelpsModel>AUART}" text="{f4HelpsModel>BEZEI}"/>                                                                                                                                                                                       
					<layoutData>                                                                                                                                                                                                                                              
						<l:GridData spanXL="3" spanL="3" spanM="6" spanS="12"/>                                                                                                                                                                                                  
					</layoutData>                                                                                                                                                                                                                                             
				</MultiInput>                                                                                                                                                                                                                                              
			</VBox>                                                                                                                                                                                                                                                     
			<VBox class="sapUiTinyMargin">                                                                                                                                                                                                                              
				<Label text="PO Type:"></Label>                                                                                                                                                                                                                            
				<MultiInput width="100%" id="idMIPoType" name="BSARK" showValueHelp="true" suggestionItems="{f4HelpsModel>/POTypeSet}"                                                                                                                                     
					valueHelpRequest="handleSalesModel($event,'POTypeSet','BSARK','VTEXT')">                                                                                                                                                                                  
					<core:Item key="{f4HelpsModel>BSARK}" text="{f4HelpsModel>VTEXT}"/>                                                                                                                                                                                       
					<layoutData>                                                                                                                                                                                                                                              
						<l:GridData spanXL="3" spanL="3" spanM="6" spanS="12"/>                                                                                                                                                                                                  
					</layoutData>                                                                                                                                                                                                                                             
				</MultiInput>                                                                                                                                                                                                                                              
			</VBox>                                                                                                                                                                                                                                                     
			<VBox class="sapUiTinyMargin">                                                                                                                                                                                                                              
				<Label text="Material Number:"></Label>                                                                                                                                                                                                                    
				<MultiInput width="100%" id="idMIMatNum" name="MATNR" showValueHelp="true" suggestionItems="{f4HelpsModel>/MaterialSet}"                                                                                                                                   
					valueHelpRequest="handleSalesModel($event,'MaterialSet','MATNR','MAKTX')">                                                                                                                                                                                
					<core:Item key="{f4HelpsModel>MATNR}" text="{f4HelpsModel>MAKTX}"/>                                                                                                                                                                                       
					<layoutData>                                                                                                                                                                                                                                              
						<l:GridData spanXL="3" spanL="3" spanM="6" spanS="12"/>                                                                                                                                                                                                  
					</layoutData>                                                                                                                                                                                                                                             
				</MultiInput>                                                                                                                                                                                                                                              
			</VBox>                                                                                                                                                                                                                                                     
			<VBox class="sapUiTinyMargin">                                                                                                                                                                                                                              
				<Label text="Payment Status:"></Label>                                                                                                                                                                                                                     
				<ComboBox width="100%" id="idCBPayStat" name="PSTAT">                                                                                                                                                                                                      
					<items>                                                                                                                                                                                                                                                   
						<core:Item key="P" text="PAID"/>                                                                                                                                                                                                                         
						<core:Item key="U" text="NOT PAID"/>                                                                                                                                                                                                                     
					</items>                                                                                                                                                                                                                                                  
					<layoutData>                                                                                                                                                                                                                                              
						<l:GridData spanXL="3" spanL="3" spanM="6" spanS="12"/>                                                                                                                                                                                                  
					</layoutData>                                                                                                                                                                                                                                             
				</ComboBox>                                                                                                                                                                                                                                                
			</VBox>                                                                                                                                                                                                                                                     
			<VBox class="sapUiTinyMargin">                                                                                                                                                                                                                              
				<Label text="Payment Terms:"></Label>                                                                                                                                                                                                                      
				<MultiInput id="idMIPayTerms" name="ZTERM" width="100%" showValueHelp="true" suggestionItems="{f4HelpsModel>/PaymentTermSet}"                                                                                                                              
					valueHelpRequest="handleSalesModel($event,'PaymentTermSet','ZTERM','TEXT1')">                                                                                                                                                                             
					<core:Item key="{f4HelpsModel>ZTERM}" text="{f4HelpsModel>TEXT1}"/>                                                                                                                                                                                       
					<layoutData>                                                                                                                                                                                                                                              
						<l:GridData spanXL="3" spanL="3" spanM="6" spanS="12"/>                                                                                                                                                                                                  
					</layoutData>                                                                                                                                                                                                                                             
				</MultiInput>                                                                                                                                                                                                                                              
			</VBox>                                                                                                                                                                                                                                                     
			<VBox class="sapUiTinyMargin">                                                                                                                                                                                                                              
				<Label text="Sold to BPID:"></Label>                                                                                                                                                                                                                       
				<MultiInput id="idMISldToBPID" name="KUNNR" width="100%" showValueHelp="true" suggestionItems="{f4HelpsModel>/SoldtoSet}"                                                                                                                                  
					valueHelpRequest="handleSalesModel($event,'SoldtoSet','KUNNR','NAME1')">                                                                                                                                                                                  
					<core:Item key="{f4HelpsModel>KUNNR}" text="{f4HelpsModel>NAME1}"/>                                                                                                                                                                                       
					<layoutData>                                                                                                                                                                                                                                              
						<l:GridData spanXL="3" spanL="3" spanM="6" spanS="12"/>                                                                                                                                                                                                  
					</layoutData>                                                                                                                                                                                                                                             
				</MultiInput>                                                                                                                                                                                                                                              
			</VBox>                                                                                                                                                                                                                                                     
			<VBox class="sapUiTinyMargin">                                                                                                                                                                                                                              
				<Label text="Ship TO BPID:"></Label>                                                                                                                                                                                                                       
				<MultiInput id="idMIShpToBPID" name="KUNWE" width="100%" showValueHelp="true" suggestionItems="{f4HelpsModel>/ShiptoSet}"                                                                                                                                  
					valueHelpRequest="handleSalesModel($event,'ShiptoSet','KUNWE','NAME1')">                                                                                                                                                                                  
					<core:Item key="{f4HelpsModel>KUNWE}" text="{f4HelpsModel>NAME1}"/>                                                                                                                                                                                       
					<layoutData>                                                                                                                                                                                                                                              
						<l:GridData spanXL="3" spanL="3" spanM="6" spanS="12"/>                                                                                                                                                                                                  
					</layoutData>                                                                                                                                                                                                                                             
				</MultiInput>                                                                                                                                                                                                                                              
			</VBox>                                                                                                                                                                                                                                                     
			<VBox class="sapUiTinyMargin">                                                                                                                                                                                                                              
				<Label text="FF BPID:"></Label>                                                                                                                                                                                                                            
				<MultiInput id="idMIFFBPID" name="KUNSP" width="100%" showValueHelp="true" suggestionItems="{f4HelpsModel>/FreightForwarderSet}"                                                                                                                           
					valueHelpRequest="handleSalesModel($event,'FreightForwarderSet','KUNSP','NAME1')">                                                                                                                                                                        
					<core:Item key="{f4HelpsModel>KUNSP}" text="{f4HelpsModel>NAME1}"/>                                                                                                                                                                                       
					<layoutData>                                                                                                                                                                                                                                              
						<l:GridData spanXL="3" spanL="3" spanM="6" spanS="12"/>                                                                                                                                                                                                  
					</layoutData>                                                                                                                                                                                                                                             
				</MultiInput>                                                                                                                                                                                                                                              
			</VBox>                                                                                                                                                                                                                                                     
			<VBox class="sapUiTinyMargin">                                                                                                                                                                                                                              
				<Label text="Society BPID:"></Label>                                                                                                                                                                                                                       
				<MultiInput id="idMISocBPID" name="KUNZA" width="100%" showValueHelp="true" suggestionItems="{f4HelpsModel>/SocietyPartnerSet}"                                                                                                                            
					valueHelpRequest="handleSalesModel($event,'SocietyPartnerSet','KUNZA','NAME1')">                                                                                                                                                                          
					<core:Item key="{f4HelpsModel>KUNZA}" text="{f4HelpsModel>NAME1}"/>                                                                                                                                                                                       
					<layoutData>                                                                                                                                                                                                                                              
						<l:GridData spanXL="3" spanL="3" spanM="6" spanS="12"/>                                                                                                                                                                                                  
					</layoutData>                                                                                                                                                                                                                                             
				</MultiInput>                                                                                                                                                                                                                                              
			</VBox>                                                                                                                                                                                                                                                     
			<VBox class="sapUiTinyMargin">                                                                                                                                                                                                                              
				<Label text="Material Group 5:"></Label>                                                                                                                                                                                                                   
				<MultiInput id="idMIMatGrp5" name="MVGR5" width="100%" showValueHelp="true" suggestionItems="{f4HelpsModel>/MaterialGroup5Set}"                                                                                                                            
					valueHelpRequest="handleSalesModel($event,'MaterialGroup5Set','MVGR5','BEZEI')">                                                                                                                                                                          
					<core:Item key="{f4HelpsModel>MVGR5}" text="{f4HelpsModel>BEZEI}"/>                                                                                                                                                                                       
					<layoutData>                                                                                                                                                                                                                                              
						<l:GridData spanXL="3" spanL="3" spanM="6" spanS="12"/>                                                                                                                                                                                                  
					</layoutData>                                                                                                                                                                                                                                             
				</MultiInput>                                                                                                                                                                                                                                              
			</VBox>                                                                                                                                                                                                                                                     
			<VBox class="sapUiTinyMargin">                                                                                                                                                                                                                              
				<Label text="Price Group:"></Label>                                                                                                                                                                                                                        
				<MultiInput id="idMIPrcGrp" name="KONDA" width="100%" showValueHelp="true" suggestionItems="{f4HelpsModel>/PriceGroupSet}"                                                                                                                                 
					valueHelpRequest="handleSalesModel($event,'PriceGroupSet','KONDA','VTEXT')">                                                                                                                                                                              
					<core:Item key="{f4HelpsModel>KONDA}" text="{f4HelpsModel>VTEXT}"/>                                                                                                                                                                                       
					<layoutData>                                                                                                                                                                                                                                              
						<l:GridData spanXL="3" spanL="3" spanM="6" spanS="12"/>                                                                                                                                                                                                  
					</layoutData>                                                                                                                                                                                                                                             
				</MultiInput>                                                                                                                                                                                                                                              
			</VBox>                                                                                                                                                                                                                                                     
			<VBox class="sapUiTinyMargin">                                                                                                                                                                                                                              
				<Label text="Sales Document:"></Label>                                                                                                                                                                                                                     
				<MultiInput id="idMISalesDoc" name="VBELN" width="100%" showValueHelp="true" suggestionItems="{f4HelpsModel>/IOrderSet}"                                                                                                                                   
					valueHelpRequest="handleSalesModel($event,'IOrderSet','VBELN','VBELN')">                                                                                                                                                                                  
					<core:Item key="{f4HelpsModel>VBELN}" text="{f4HelpsModel>VBELN}"/>                                                                                                                                                                                       
					<layoutData>                                                                                                                                                                                                                                              
						<l:GridData spanXL="3" spanL="3" spanM="6" spanS="12"/>                                                                                                                                                                                                  
					</layoutData>                                                                                                                                                                                                                                             
				</MultiInput>                                                                                                                                                                                                                                              
			</VBox>                                                                                                                                                                                                                                                     
		</l:Grid>                                                                                                                                                                                                                                                    
		<buttons>                                                                                                                                                                                                                                                    
			<Button text="CLEAR" press="onFilterDialogClear"></Button>                                                                                                                                                                                                  
			<Button type="Accept" text="GO" press="onFilterDialogGo"></Button>                                                                                                                                                                                          
			<Button type="Reject" text="CLOSE" press="onFilterDialogClose"></Button>                                                                                                                                                                                    
		</buttons>                                                                                                                                                                                                                                                   
	</Dialog>                                                                                                                                                                                                                                                     
</core:FragmentDefinition>                                                                                                                                                                                                                                     