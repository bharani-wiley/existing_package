     </IMPORT>
     <EXPORT>
      <RSEXP>
       <PARAMETER>EX_IS_ERROR</PARAMETER>
       <REFERENCE>X</REFERENCE>
       <TYP>FLAG</TYP>
      </RSEXP>
      <RSEXP>
       <PARAMETER>EX_MESSAGE_TAB</PARAMETER>
       <REFERENCE>X</REFERENCE>
       <TYP>MERRDAT_TT</TYP>
      </RSEXP>
     </EXPORT>
     <EXCEPTION>
      <RSEXC>
       <EXCEPTION>EXC_MED_PROD_INVALID</EXCEPTION>
      </RSEXC>
      <RSEXC>
       <EXCEPTION>EXC_MED_PROD_LOCKED</EXCEPTION>
      </RSEXC>
      <RSEXC>
       <EXCEPTION>EXC_TEMP_ISSUE_MISSING</EXCEPTION>
      </RSEXC>
     </EXCEPTION>
     <DOCUMENTATION>
      <RSFDO>
       <PARAMETER>IM_MED_ISSUE_MARA</PARAMETER>
       <KIND>P</KIND>
       <STEXT>I0344: Media Issue - General Material Data from JANIS</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>IM_MED_ISSUE_MAKT</PARAMETER>
       <KIND>P</KIND>
       <STEXT>I0344: Media Issue - Material Description from JANIS</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>IM_MED_ISSUE_MARC</PARAMETER>
       <KIND>P</KIND>
       <STEXT>I0344: Media Issue - Plant Data for Material from JANIS</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>IM_MED_ISSUE_MVKE</PARAMETER>
       <KIND>P</KIND>
       <STEXT>I0344: Media Issue - Sales Data for Material from JANIS</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>IM_MED_ISSUE_IDCD</PARAMETER>
       <KIND>P</KIND>
