      </RSEXP>
     </EXPORT>
     <EXCEPTION>
      <RSEXC>
       <EXCEPTION>EXC_TEMP_ISSUE_MISSING</EXCEPTION>
      </RSEXC>
     </EXCEPTION>
     <DOCUMENTATION>
      <RSFDO>
       <PARAMETER>IM_MED_PRODUCT</PARAMETER>
       <KIND>P</KIND>
