  ELSEIF X_HEADER-DELMDTFLAG NE SPACE.
    PERFORM BUILD_MAINKEY_TAB.
  ENDIF.
  REFRESH EXTRACT.
ENDFORM.
*---------------------------------------------------------------------*
FORM DB_UPD_ZQTCV_PRINT_VEND .
*.process data base updates/inserts/deletes.........................*
LOOP AT TOTAL.
  CHECK <ACTION> NE ORIGINAL.
MOVE <VIM_TOTAL_STRUC> TO ZQTCV_PRINT_VEND.
  IF <ACTION> = UPDATE_GELOESCHT.
    <ACTION> = GELOESCHT.
  ENDIF.
  CASE <ACTION>.
   WHEN NEUER_GELOESCHT.
IF STATUS_ZQTCV_PRINT_VEND-ST_DELETE EQ GELOESCHT.
     READ TABLE EXTRACT WITH KEY <VIM_XTOTAL_KEY>.
     IF SY-SUBRC EQ 0.
       DELETE EXTRACT INDEX SY-TABIX.
     ENDIF.
    ENDIF.
    DELETE TOTAL.
    IF X_HEADER-DELMDTFLAG NE SPACE.
      PERFORM DELETE_FROM_MAINKEY_TAB.
    ENDIF.
   WHEN GELOESCHT.
  SELECT SINGLE FOR UPDATE * FROM ZQTC_PRINT_VEND WHERE
  BUS_PRCOCESS = ZQTCV_PRINT_VEND-BUS_PRCOCESS AND
  COUNTRY = ZQTCV_PRINT_VEND-COUNTRY AND
  OUTPUT_TYPE = ZQTCV_PRINT_VEND-OUTPUT_TYPE .
    IF SY-SUBRC = 0.
    DELETE ZQTC_PRINT_VEND .
    ENDIF.
    IF STATUS-DELETE EQ GELOESCHT.
      READ TABLE EXTRACT WITH KEY <VIM_XTOTAL_KEY> BINARY SEARCH.
      DELETE EXTRACT INDEX SY-TABIX.
    ENDIF.
    DELETE TOTAL.
    IF X_HEADER-DELMDTFLAG NE SPACE.
      PERFORM DELETE_FROM_MAINKEY_TAB.
    ENDIF.
   WHEN OTHERS.
  SELECT SINGLE FOR UPDATE * FROM ZQTC_PRINT_VEND WHERE
  BUS_PRCOCESS = ZQTCV_PRINT_VEND-BUS_PRCOCESS AND
  COUNTRY = ZQTCV_PRINT_VEND-COUNTRY AND
  OUTPUT_TYPE = ZQTCV_PRINT_VEND-OUTPUT_TYPE .
    IF SY-SUBRC <> 0.   "insert preprocessing: init WA
      CLEAR ZQTC_PRINT_VEND.
    ENDIF.
ZQTC_PRINT_VEND-MANDT =
ZQTCV_PRINT_VEND-MANDT .
ZQTC_PRINT_VEND-BUS_PRCOCESS =
ZQTCV_PRINT_VEND-BUS_PRCOCESS .
ZQTC_PRINT_VEND-COUNTRY =
ZQTCV_PRINT_VEND-COUNTRY .
ZQTC_PRINT_VEND-OUTPUT_TYPE =
ZQTCV_PRINT_VEND-OUTPUT_TYPE .
ZQTC_PRINT_VEND-PRINT_VENDOR =
ZQTCV_PRINT_VEND-PRINT_VENDOR .
ZQTC_PRINT_VEND-PRINT_REGION =
ZQTCV_PRINT_VEND-PRINT_REGION .
ZQTC_PRINT_VEND-COUNTRY_SORT =
ZQTCV_PRINT_VEND-COUNTRY_SORT .
ZQTC_PRINT_VEND-FILE_LOC =
ZQTCV_PRINT_VEND-FILE_LOC .
ZQTC_PRINT_VEND-AENAM =
ZQTCV_PRINT_VEND-AENAM .
ZQTC_PRINT_VEND-AEDAT =
ZQTCV_PRINT_VEND-AEDAT .
ZQTC_PRINT_VEND-AEZET =
ZQTCV_PRINT_VEND-AEZET .
    IF SY-SUBRC = 0.
    UPDATE ZQTC_PRINT_VEND .
    ELSE.
    INSERT ZQTC_PRINT_VEND .
    ENDIF.
    READ TABLE EXTRACT WITH KEY <VIM_XTOTAL_KEY>.
    IF SY-SUBRC EQ 0.
      <XACT> = ORIGINAL.
      MODIFY EXTRACT INDEX SY-TABIX.
    ENDIF.
    <ACTION> = ORIGINAL.
    MODIFY TOTAL.
  ENDCASE.
ENDLOOP.
CLEAR: STATUS_ZQTCV_PRINT_VEND-UPD_FLAG,
STATUS_ZQTCV_PRINT_VEND-UPD_CHECKD.
MESSAGE S018(SV).
ENDFORM.
