<core:FragmentDefinition                                                                                                                                                                                                                                       
	xmlns="sap.m"                                                                                                                                                                                                                                                 
	xmlns:core="sap.ui.core">                                                                                                                                                                                                                                     
	<SelectDialog                                                                                                                                                                                                                                                 
		id="valueHelpDialog"                                                                                                                                                                                                                                         
		title="SoldTo ValueHelp"                                                                                                                                                                                                                                     
		items="{/SOLDTOSet}"                                                                                                                                                                                                                                         
		search="handleSoldToRptSearch"                                                                                                                                                                                                                               
		confirm="handleSoldToRptClose"                                                                                                                                                                                                                               
		cancel="handleSoldToRptClose"                                                                                                                                                                                                                                
		multiSelect="true">                                                                                                                                                                                                                                          
		<StandardListItem                                                                                                                                                                                                                                            
		    id="soldTolistRpt"                                                                                                                                                                                                                                       
			title="{Kunnr}"                                                                                                                                                                                                                                             
			info="{Name2}"                                                                                                                                                                                                                                              
			description="{Name1}"/>                                                                                                                                                                                                                                     
	</SelectDialog>                                                                                                                                                                                                                                               
</core:FragmentDefinition>                                                                                                                                                                                                                                     