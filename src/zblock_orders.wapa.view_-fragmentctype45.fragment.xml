<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" controllerName="Block_Orders.Block_Orders.controller.Worklist">                                                                                                                                
	<Dialog id="dct45" resizable="true" draggable="true" contentWidth="500px">                                                                                                                                                                                    
		<content>                                                                                                                                                                                                                                                    
			<Toolbar id="ct21">                                                                                                                                                                                                                                         
				<OverflowToolbar id="ct22">                                                                                                                                                                                                                                
					<SearchField id="mytypSearc45h" search="searchCT45EH" width="250px"/>                                                                                                                                                                                     
				</OverflowToolbar>                                                                                                                                                                                                                                         
				<ToolbarSpacer id="ct23"/>                                                                                                                                                                                                                                 
				<Button id="ct24" icon="sap-icon://clear-filter" press="clearCT45" type="Accept" tooltip="Clear Selected Types" width="80px"/>                                                                                                                             
			</Toolbar>                                                                                                                                                                                                                                                  
				<Panel id="CT45Panel" expandable="true" expanded="true" headerText="Selected Items">                                                                                                                                                                       
				<Table items="{CT45sel>/}" id="CT45selTable" growing="true" mode="Delete"                                                                                                                                                                                  
				delete="deleteSelectionCT45" showNoData="false" >                                                                                                                                                                                                          
					<items>                                                                                                                                                                                                                                                   
						<ColumnListItem id="ct25">                                                                                                                                                                                                                               
							<cells>                                                                                                                                                                                                                                                 
								<Text id="ct26" text="{CT45sel>Auart}"/>                                                                                                                                                                                                               
							</cells>                                                                                                                                                                                                                                                
						</ColumnListItem>                                                                                                                                                                                                                                        
						</items>                                                                                                                                                                                                                                                 
						<columns>                                                                                                                                                                                                                                                
							<Column id="ct27" width="150px">                                                                                                                                                                                                                        
								<!--<Text text="{CT45sel>Kunnr}"/>-->                                                                                                                                                                                                                  
							</Column>                                                                                                                                                                                                                                               
						</columns>                                                                                                                                                                                                                                               
					</Table>                                                                                                                                                                                                                                                  
				</Panel>                                                                                                                                                                                                                                                   
			<Table items="{Ctypes45>/results}" id="CT45Table" growing="true" growingThreshold="75" mode="MultiSelect"                                                                                                                                                   
			       	selectionChange="rowSelectionCT45" >                                                                                                                                                                                                                
				<items>                                                                                                                                                                                                                                                    
					<ColumnListItem id="ct28">                                                                                                                                                                                                                                
						<cells>                                                                                                                                                                                                                                                  
							<Text id="otct1" text="{Ctypes45>Auart}"/>                                                                                                                                                                                                              
								<Text id="otct2" text="{Ctypes45>Bezei}"/>                                                                                                                                                                                                             
						</cells>                                                                                                                                                                                                                                                 
					</ColumnListItem>                                                                                                                                                                                                                                         
				</items>                                                                                                                                                                                                                                                   
				<columns>                                                                                                                                                                                                                                                  
					<Column id="ct29" width="70px">                                                                                                                                                                                                                           
						<Label id = "ltct1" text="Contract Type"/>                                                                                                                                                                                                               
						</Column>                                                                                                                                                                                                                                                
				   <Column id="ct30" width="130px">                                                                                                                                                                                                                        
						<Label id="ltct2" text="Description"/>                                                                                                                                                                                                                   
					</Column>                                                                                                                                                                                                                                                 
                                                                                                                                                                                                                                                               
				</columns>                                                                                                                                                                                                                                                 
			</Table>                                                                                                                                                                                                                                                    
					</content>                                                                                                                                                                                                                                                
			<beginButton>                                                                                                                                                                                                                                               
				<Button id="ct31" icon="sap-icon://accept" press="acceptCT45" type="Accept" tooltip="Select"/>                                                                                                                                                             
			</beginButton>                                                                                                                                                                                                                                              
			<endButton text="test">                                                                                                                                                                                                                                     
				<Button id="ct32" icon="sap-icon://sys-cancel" press="cancelCT45" type="Reject" tooltip="close"/>                                                                                                                                                          
			</endButton>                                                                                                                                                                                                                                                
		</Dialog>                                                                                                                                                                                                                                                    
	</core:FragmentDefinition>                                                                                                                                                                                                                                    