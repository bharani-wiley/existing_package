			</table:template>                                                                                                                                                                                                                                           
			<table:template>                                                                                                                                                                                                                                            
				<ObjectNumber id="obj11"                                                                                                                                                                                                                                   
					number="{ parts: [{path:'Dunning>InvoiceValue'}], type: 'sap.ui.model.type.Currency', formatOptions: {showMeasure: false} }"                                                                                                                              
					unit="{Dunning>Currency}" textAlign="Right"/>                                                                                                                                                                                                             
			</table:template>                                                                                                                                                                                                                                           
		</table:Column>                                                                                                                                                                                                                                              
		<table:Column width="85px" sortProperty="Matnr" filterProperty="Matnr">                                                                                                                                                                                      
