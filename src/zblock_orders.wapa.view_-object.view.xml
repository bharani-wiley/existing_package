				<Label text="Ship To" design="Bold"/>                                                                                                                                                                                                                      
			</table:label>                                                                                                                                                                                                                                              
			<table:template>                                                                                                                                                                                                                                            
				<Link text="{Dunning>ShipTo}" href="{Dunning>Linkref3}" target="_blank"/>                                                                                                                                                                                  
			</table:template>                                                                                                                                                                                                                                           
		</table:Column>                                                                                                                                                                                                                                              
		<table:Column width="110px" sortProperty="InvoiceValue" filterProperty="InvoiceValue" autoResizable="true">                                                                                                                                                  
