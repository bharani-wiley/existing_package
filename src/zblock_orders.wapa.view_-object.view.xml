			</table:template>                                                                                                                                                                                                                                           
		</table:Column>                                                                                                                                                                                                                                              
		<table:Column width="120px" sortProperty="DelvBlock" filterProperty="DelvBlock" autoResizable="true">                                                                                                                                                        
