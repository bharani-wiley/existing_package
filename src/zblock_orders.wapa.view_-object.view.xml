				<Label text="Item#" design="Bold"/>                                                                                                                                                                                                                        
			</table:label>                                                                                                                                                                                                                                              
			<table:template>                                                                                                                                                                                                                                            
				<Label text="{Dunning>Posnr}" />                                                                                                                                                                                                                           
			</table:template>                                                                                                                                                                                                                                           
		</table:Column>                                                                                                                                                                                                                                              
		<table:Column width="95px" sortProperty="ShipTo" filterProperty="ShipTo" autoResizable="true">                                                                                                                                                               
