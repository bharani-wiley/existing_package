				<Link text="{Dunning>Vbeln}" href="{Dunning>Linkref1}" target="_blank"/>                                                                                                                                                                                   
			</table:template>                                                                                                                                                                                                                                           
		</table:Column>                                                                                                                                                                                                                                              
