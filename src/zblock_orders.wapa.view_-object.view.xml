	xmlns:smartTable="sap.ui.comp.smarttable">                                                                                                                                                                                                                    
	<HBox id="HB1">                                                                                                                                                                                                                                               
		<Image id="IM1" src="Logo/Wileylogo.GIF" height="50px" width="140px"/>                                                                                                                                                                                       
		<Text id="HT1" text="Delivery Management Report" textAlign="Center" width="925px" class="styleclass1"/>                                                                                                                                                      
		<Text id="CS1" text="." textAlign="Left" width="200px" class="styleclass5"/>                                                                                                                                                                                 
	</HBox>                                                                                                                                                                                                                                                       
	<table:Table id="seltable" rows="{Dunning>/results}" rowSelectionChange="rowSelection" showNoData="false" enableGrouping="true"                                                                                                                               
		alternateRowColors="true" selectionMode="MultiToggle" enableSelectAll="true" visibleRowCount="10" cellClick="Menu">                                                                                                                                          
		<table:extension>                                                                                                                                                                                                                                            
			<OverflowToolbar id="infobar">                                                                                                                                                                                                                              
				<Button icon="sap-icon://nav-back" tooltip="Back" press="onNavBack"></Button>                                                                                                                                                                              
				<Button id="Release" type="Reject" icon="sap-icon://locked" tooltip="Delivery Block" press="OnBlock"></Button>                                                                                                                                             
				<Button id="btnMsgPopover" text="View Message Log" type="Success" press="onMessagePopoverPress" visible="false"/>                                                                                                                                          
				<SearchField id="searchField" tooltip="{Search}" search="onMainSearch" width="250px"></SearchField>                                                                                                                                                        
				<ToolbarSpacer/>                                                                                                                                                                                                                                           
				<Title text="Total No.of Records:"/>                                                                                                                                                                                                                       
				<Title id="getrecords"/>                                                                                                                                                                                                                                   
				<Text text=""/>                                                                                                                                                                                                                                            
				<Button id="Selection" tooltip="Selection Filter" icon="sap-icon://filter" type="Emphasized" press="OnSel"/>                                                                                                                                               
				<Button icon="sap-icon://action-settings" tooltip="Layout Settings" press="onPersoButtonPressed29" type="Emphasized"/>                                                                                                                                     
				<Button icon="sap-icon://excel-attachment" tooltip="Download Excel" type="Emphasized" press="downloadExcel"/>                                                                                                                                              
			</OverflowToolbar>                                                                                                                                                                                                                                          
		</table:extension>                                                                                                                                                                                                                                           
		<table:Column width="95px" sortProperty="Kunnr" filterProperty="Kunnr" autoResizable="true">                                                                                                                                                                 
			<table:label>                                                                                                                                                                                                                                               
				<Label text="Sold to BP" design="Bold"/>                                                                                                                                                                                                                   
			</table:label>                                                                                                                                                                                                                                              
			<table:template>                                                                                                                                                                                                                                            
				<Link text="{Dunning>Kunnr}" href="{Dunning>Linkref2}" target="_blank"/>                                                                                                                                                                                   
			</table:template>                                                                                                                                                                                                                                           
		</table:Column>                                                                                                                                                                                                                                              
		 <table:Column width="100px" sortProperty="Name1" filterProperty="Name" autoResizable="true">                                                                                                                                                                
			<table:label>                                                                                                                                                                                                                                               
				<Label text="Name" design="Bold"/>                                                                                                                                                                                                                         
			</table:label>                                                                                                                                                                                                                                              
			<table:template>                                                                                                                                                                                                                                            
				<Label text="{Dunning>Name1}" />                                                                                                                                                                                                                           
			</table:template>                                                                                                                                                                                                                                           
		</table:Column>                                                                                                                                                                                                                                              
		<table:Column width="130px" autoResizable="true">                                                                                                                                                                                                            
			<table:label>                                                                                                                                                                                                                                               
				<Label text="Sold To Address" design="Bold"/>                                                                                                                                                                                                              
			</table:label>                                                                                                                                                                                                                                              
			<table:template>                                                                                                                                                                                                                                            
				<VBox>                                                                                                                                                                                                                                                     
					<items>                                                                                                                                                                                                                                                   
						<Button text="Soldto Address" id="soldToAdd" press="soldtoAdd"/>                                                                                                                                                                                         
					</items>                                                                                                                                                                                                                                                  
				</VBox>                                                                                                                                                                                                                                                    
			</table:template>                                                                                                                                                                                                                                           
		</table:Column>                                                                                                                                                                                                                                              
