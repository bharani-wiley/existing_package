			</table:template>                                                                                                                                                                                                                                           
		</table:Column>                                                                                                                                                                                                                                              
		<table:Column width="60px" sortProperty="Auart" filterProperty="Auart" autoResizable="true">                                                                                                                                                                 
			<table:label>                                                                                                                                                                                                                                               
				<Label text="Sales Order Type" design="Bold"/>                                                                                                                                                                                                             
			</table:label>                                                                                                                                                                                                                                              
			<table:template>                                                                                                                                                                                                                                            
				<Label text="{Dunning>Auart}"/>                                                                                                                                                                                                                            
			</table:template>                                                                                                                                                                                                                                           
		</table:Column>                                                                                                                                                                                                                                              
