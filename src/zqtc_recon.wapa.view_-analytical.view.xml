											<DatePicker id="IDateto" valueFormat="yyyy-MM-dd" change="handleChange" displayFormat="long" textAlign="Center" textDirection="RTL"></DatePicker>                                                                                                   
											<Text text=" " width="10px"/>                                                                                                                                                                                                                       
											<MultiComboBox selectionChange="handleSelectionChange" items="{/DevIdSet}" selectionFinish="handleSelectionFinish" width="350px"                                                                                                                    
												showSecondaryValues="true" placeholder="Select Interface Id's">                                                                                                                                                                                    
												<core:ListItem key="{Devid}" text="{Devid}" additionalText="{Description}"/>                                                                                                                                                                       
											</MultiComboBox>                                                                                                                                                                                                                                    
											<Text text=" " width="10px"/>                                                                                                                                                                                                                       
