						<HBox>                                                                                                                                                                                                                                                   
							<Image src="Logo/Wileylogo.GIF" height="50px"/>                                                                                                                                                                                                         
							<Text text="Reconciliation Dashboard" textAlign="Center" width="1150px" class="styleclass1"/>                                                                                                                                                           
							<!--<Text text=". " textAlign="End" width="200px" class="styleclass1"/>-->                                                                                                                                                                              
						</HBox>                                                                                                                                                                                                                                                  
					</headerContent>                                                                                                                                                                                                                                          
					<content>                                                                                                                                                                                                                                                 
						<!--<HBox>-->                                                                                                                                                                                                                                            
						<!--<Image src="Logo/Wileylogo.GIF" height="50px" width="150px"/>-->                                                                                                                                                                                     
