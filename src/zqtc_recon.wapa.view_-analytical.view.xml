										</HBox>                                                                                                                                                                                                                                              
									</HBox>                                                                                                                                                                                                                                               
									<viz:Popover id="idPopOver"></viz:Popover>                                                                                                                                                                                                            
									<viz:VizFrame width="100%" id="idVizFrame" uiConfig="{applicationSet:'fiori'}" vizType='stacked_bar' selectData="onSelectData"                                                                                                                        
										vizProperties="{dataLabel:{ showTotal: true},title:{ text:'Error Analysis'},interaction: {selectability: {mode: 'EXCLUSIVE'}}}">                                                                                                                     
										<viz:dataset>                                                                                                                                                                                                                                        
											<viz.data:FlattenedDataset id="idVizDataSet" data="{main>/results}">                                                                                                                                                                                
												<viz.data:dimensions>                                                                                                                                                                                                                              
													<viz.data:DimensionDefinition name="DevID" value="{main>Devid}"/>                                                                                                                                                                                 
												</viz.data:dimensions>                                                                                                                                                                                                                             
												<viz.data:measures>                                                                                                                                                                                                                                
													<viz.data:MeasureDefinition name="success" value="{main>Success}"/>                                                                                                                                                                               
													<viz.data:MeasureDefinition name="error" value="{main>Error}"/>                                                                                                                                                                                   
												</viz.data:measures>                                                                                                                                                                                                                               
											</viz.data:FlattenedDataset>                                                                                                                                                                                                                        
										</viz:dataset>                                                                                                                                                                                                                                       
										<viz:feeds>                                                                                                                                                                                                                                          
											<viz.feeds:FeedItem id='valueAxisFeed' uid="valueAxis" type="Measure" values="success,error"/>                                                                                                                                                      
											<viz.feeds:FeedItem uid="categoryAxis" type="Dimension" values="DevID"/>                                                                                                                                                                            
										</viz:feeds>                                                                                                                                                                                                                                         
									</viz:VizFrame>                                                                                                                                                                                                                                       
								</IconTabFilter>                                                                                                                                                                                                                                       
