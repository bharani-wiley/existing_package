						</cells>                                                                                                                                                                                                                                                 
					</ColumnListItem>                                                                                                                                                                                                                                         
				</items>                                                                                                                                                                                                                                                   
				<columns>                                                                                                                                                                                                                                                  
					<Column id="BP4529" width="70px">                                                                                                                                                                                                                         
						<Label id = "ltBP451" text="Customer"/>                                                                                                                                                                                                                  
						</Column>                                                                                                                                                                                                                                                
				   <Column id="BP4530" width="130px">                                                                                                                                                                                                                      
						<Label id="ltBP452" text="Name"/>                                                                                                                                                                                                                        
					</Column>                                                                                                                                                                                                                                                 
                                                                                                                                                                                                                                                               
				</columns>                                                                                                                                                                                                                                                 
			</Table>                                                                                                                                                                                                                                                    
					</content>                                                                                                                                                                                                                                                
			<beginButton>                                                                                                                                                                                                                                               
