							</cells>                                                                                                                                                                                                                                                
						</ColumnListItem>                                                                                                                                                                                                                                        
						</items>                                                                                                                                                                                                                                                 
						<columns>                                                                                                                                                                                                                                                
							<Column id="BP4527" width="150px">                                                                                                                                                                                                                      
								<!--<Text text="{BP4545sel>Kunnr}"/>-->                                                                                                                                                                                                                
							</Column>                                                                                                                                                                                                                                               
						</columns>                                                                                                                                                                                                                                               
					</Table>                                                                                                                                                                                                                                                  
				</Panel>                                                                                                                                                                                                                                                   
			<Table items="{/SoldtoSet}" id="BP45Table" growing="true" growingThreshold="75" mode="MultiSelect"                                                                                                                                                          
			       	selectionChange="rowSelectionBP45" >                                                                                                                                                                                                                
