      <RSIMP>
       <PARAMETER>IM_FILENAME</PARAMETER>
       <OPTIONAL>X</OPTIONAL>
       <TYP>FILE_NAME</TYP>
      </RSIMP>
      <RSIMP>
       <PARAMETER>IM_PATH</PARAMETER>
       <TYP>PATHEXTERN</TYP>
