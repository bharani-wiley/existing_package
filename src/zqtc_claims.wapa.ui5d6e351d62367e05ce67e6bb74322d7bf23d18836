			//////Media Issue Popup on claims report                                                                                                                                                                                                                    
			issueRptTab: function (oEvent) {                                                                                                                                                                                                                            
				var matnr = oEvent.getSource().getBindingContext("rpt").getProperty("Matnr");                                                                                                                                                                              
				var issuePath = oEvent.getSource().getBindingContext("rpt").getPath();                                                                                                                                                                                     
				if (!this.fragmentIssueAdd) {                                                                                                                                                                                                                              
