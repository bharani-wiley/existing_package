			panelText: function (oEvent) {                                                                                                                                                                                                                              
                                                                                                                                                                                                                                                               
				if (oEvent.getParameter("expand") == true) {                                                                                                                                                                                                               
					this.getView().byId("panel").setHeaderText("Claims Statistics");                                                                                                                                                                                          
					this.getView().byId("tableText").setVisible(true);                                                                                                                                                                                                        
				} else {                                                                                                                                                                                                                                                   
					this.getView().byId("panel").setHeaderText("List Of Release Orders");                                                                                                                                                                                     
					this.getView().byId("tableText").setVisible(false);                                                                                                                                                                                                       
				}                                                                                                                                                                                                                                                          
			},                                                                                                                                                                                                                                                          
			/////////////////////////Calims Report Tab///////////////////////////////////                                                                                                                                                                               
			////////////////////////////////////////////////////////////////////////////                                                                                                                                                                                
			///////////Sold To value help on Claims report                                                                                                                                                                                                              
			helpSoldtoRpt: function (oEvent) {                                                                                                                                                                                                                          
				//	this.getView().setBusy(true);                                                                                                                                                                                                                           
				//	var that = this;                                                                                                                                                                                                                                        
				var sInputValue = oEvent.getSource().getValue(),                                                                                                                                                                                                           
					oView = this.getView();                                                                                                                                                                                                                                   
                                                                                                                                                                                                                                                               
				// create value help dialog                                                                                                                                                                                                                                
				if (!this.pValueHelpDialog) {                                                                                                                                                                                                                              
					this.pValueHelpDialog = Fragment.load({                                                                                                                                                                                                                   
						id: "soldtoRpt",                                                                                                                                                                                                                                         
						name: "zqtc.ZQTC_CLAIMS.view.fragmentSoldtoRpt",                                                                                                                                                                                                         
						controller: this                                                                                                                                                                                                                                         
					}).then(function (oValueHelpDialog) {                                                                                                                                                                                                                     
						//	that.getView().setBusy(false);                                                                                                                                                                                                                        
						oView.addDependent(oValueHelpDialog);                                                                                                                                                                                                                    
						return oValueHelpDialog;                                                                                                                                                                                                                                 
					});                                                                                                                                                                                                                                                       
				}                                                                                                                                                                                                                                                          
				//	that.getView().setBusy(true);                                                                                                                                                                                                                           
				this.pValueHelpDialog.then(function (oValueHelpDialog) {                                                                                                                                                                                                   
					// create a filter for the binding                                                                                                                                                                                                                        
					if (sInputValue) {                                                                                                                                                                                                                                        
						var oFilter = new Filter("Name1", FilterOperator.Contains, sInputValue);                                                                                                                                                                                 
						// var oFilter2 = new Filter("Name2",FilterOperator.Contains,sInputValue);                                                                                                                                                                               
						// var oFilter3 = new Filter("Kunnr",FilterOperator.Contains,sInputValue);                                                                                                                                                                               
                                                                                                                                                                                                                                                               
						oValueHelpDialog.getBinding("items").filter([                                                                                                                                                                                                            
							oFilter                                                                                                                                                                                                                                                 
						]);                                                                                                                                                                                                                                                      
					}                                                                                                                                                                                                                                                         
					//	that.getView().setBusy(false);                                                                                                                                                                                                                         
					// open value help dialog filtered by the input value                                                                                                                                                                                                     
					oValueHelpDialog.open(sInputValue);                                                                                                                                                                                                                       
                                                                                                                                                                                                                                                               
				});                                                                                                                                                                                                                                                        
			},                                                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
			handleSoldToRptSearch: function (evt) {                                                                                                                                                                                                                     
				var sValue = evt.getParameter("value");                                                                                                                                                                                                                    
				var oFilter = new Filter("Name1", FilterOperator.EQ, sValue);                                                                                                                                                                                              
				// var oFilter2 = new Filter("Name2", FilterOperator.Contains, sValue);                                                                                                                                                                                    
				// var oFilter3 = new Filter("Kunnr", FilterOperator.Contains, sValue);                                                                                                                                                                                    
                                                                                                                                                                                                                                                               
				evt.getSource().getBinding("items").filter([oFilter]);                                                                                                                                                                                                     
			},                                                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
			handleSoldToRptClose: function (evt) {                                                                                                                                                                                                                      
				var aSelectedItems = evt.getParameter("selectedItems"),                                                                                                                                                                                                    
					oMultiInput3 = this.getView().byId("multiInput3");                                                                                                                                                                                                        
                                                                                                                                                                                                                                                               
				if (aSelectedItems && aSelectedItems.length > 0) {                                                                                                                                                                                                         
					aSelectedItems.forEach(function (oItem) {                                                                                                                                                                                                                 
						oMultiInput3.addToken(new sap.m.Token({                                                                                                                                                                                                                  
							text: oItem.getTitle()                                                                                                                                                                                                                                  
						}));                                                                                                                                                                                                                                                     
					});                                                                                                                                                                                                                                                       
				}                                                                                                                                                                                                                                                          
			},                                                                                                                                                                                                                                                          
			///////////////Shipto Value help on Claims report                                                                                                                                                                                                           
			helpShiptoRpt: function (oEvt) {                                                                                                                                                                                                                            
				//	this.getView().setBusy(true);                                                                                                                                                                                                                           
				var that = this;                                                                                                                                                                                                                                           
				var sInputValue = oEvt.getSource().getValue(),                                                                                                                                                                                                             
					oView = this.getView();                                                                                                                                                                                                                                   
                                                                                                                                                                                                                                                               
				// create value help dialog                                                                                                                                                                                                                                
				if (!this.sValueHelpDialog) {                                                                                                                                                                                                                              
					this.sValueHelpDialog = Fragment.load({                                                                                                                                                                                                                   
						id: "shiptoRpt",                                                                                                                                                                                                                                         
						name: "zqtc.ZQTC_CLAIMS.view.fragmentShiptoRpt",                                                                                                                                                                                                         
						controller: this                                                                                                                                                                                                                                         
					}).then(function (aValueHelpDialog) {                                                                                                                                                                                                                     
						//			that.getView().setBusy(false);                                                                                                                                                                                                                      
						oView.addDependent(aValueHelpDialog);                                                                                                                                                                                                                    
						return aValueHelpDialog;                                                                                                                                                                                                                                 
					});                                                                                                                                                                                                                                                       
				}                                                                                                                                                                                                                                                          
				that.getView().setBusy(false);                                                                                                                                                                                                                             
				this.sValueHelpDialog.then(function (aValueHelpDialog) {                                                                                                                                                                                                   
					// create a filter for the binding                                                                                                                                                                                                                        
					if (sInputValue) {                                                                                                                                                                                                                                        
						var oFilter = new Filter("Name1", FilterOperator.Contains, sInputValue);                                                                                                                                                                                 
                                                                                                                                                                                                                                                               
						aValueHelpDialog.getBinding("items").filter([                                                                                                                                                                                                            
							oFilter                                                                                                                                                                                                                                                 
						]);                                                                                                                                                                                                                                                      
					}                                                                                                                                                                                                                                                         
					// open value help dialog filtered by the input value                                                                                                                                                                                                     
					aValueHelpDialog.open(sInputValue);                                                                                                                                                                                                                       
					//	that.getView().setBusy(false);                                                                                                                                                                                                                         
				});                                                                                                                                                                                                                                                        
                                                                                                                                                                                                                                                               
			},                                                                                                                                                                                                                                                          
			handleShipToRptSearch: function (evt) {                                                                                                                                                                                                                     
				var sValue = evt.getParameter("value");                                                                                                                                                                                                                    
				var oFilter = new Filter("Name1", FilterOperator.EQ, sValue);                                                                                                                                                                                              
				// var oFilter2 = new Filter("Name2", FilterOperator.Contains, sValue);                                                                                                                                                                                    
				// var oFilter3 = new Filter("Shipto", FilterOperator.Contains, sValue);                                                                                                                                                                                   
                                                                                                                                                                                                                                                               
				evt.getSource().getBinding("items").filter([oFilter]);                                                                                                                                                                                                     
			},                                                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
			handleShipToRptClose: function (evt) {                                                                                                                                                                                                                      
				var aSelectedItems = evt.getParameter("selectedItems"),                                                                                                                                                                                                    
					oMultiInput3 = this.getView().byId("multiInput4");                                                                                                                                                                                                        
                                                                                                                                                                                                                                                               
				if (aSelectedItems && aSelectedItems.length > 0) {                                                                                                                                                                                                         
					aSelectedItems.forEach(function (oItem) {                                                                                                                                                                                                                 
						oMultiInput3.addToken(new sap.m.Token({                                                                                                                                                                                                                  
							text: oItem.getTitle()                                                                                                                                                                                                                                  
						}));                                                                                                                                                                                                                                                     
					});                                                                                                                                                                                                                                                       
				}                                                                                                                                                                                                                                                          
			},                                                                                                                                                                                                                                                          
			//////Shipped to Country value help on claims report                                                                                                                                                                                                        
			helpCtryRpt: function (oEvt) {                                                                                                                                                                                                                              
				var sInputValue = oEvt.getSource().getValue(),                                                                                                                                                                                                             
					oView = this.getView();                                                                                                                                                                                                                                   
                                                                                                                                                                                                                                                               
				// create value help dialog                                                                                                                                                                                                                                
				if (!this.cValueHelpDialog) {                                                                                                                                                                                                                              
					this.cValueHelpDialog = Fragment.load({                                                                                                                                                                                                                   
						id: "cntryRpt",                                                                                                                                                                                                                                          
						name: "zqtc.ZQTC_CLAIMS.view.fragmentShipCtry",                                                                                                                                                                                                          
						controller: this                                                                                                                                                                                                                                         
