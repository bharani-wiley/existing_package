			},                                                                                                                                                                                                                                                          
			downloadExcelRpt: function () {                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                               
				var aCols, aRpt, oSettings, oSheet;                                                                                                                                                                                                                        
                                                                                                                                                                                                                                                               
				aCols = this.createColumnConfig();                                                                                                                                                                                                                         
                                                                                                                                                                                                                                                               
				aRpt = this.getView().getModel("rpt").getProperty("/results");                                                                                                                                                                                             
                                                                                                                                                                                                                                                               
				oSettings = {                                                                                                                                                                                                                                              
					workbook: {                                                                                                                                                                                                                                               
						columns: aCols                                                                                                                                                                                                                                           
					},                                                                                                                                                                                                                                                        
					dataSource: aRpt                                                                                                                                                                                                                                          
				};                                                                                                                                                                                                                                                         
                                                                                                                                                                                                                                                               
				oSheet = new Spreadsheet(oSettings);                                                                                                                                                                                                                       
				oSheet.build()                                                                                                                                                                                                                                             
					.then(function () {                                                                                                                                                                                                                                       
						sap.m.MessageToast.show("Excel download has finished");                                                                                                                                                                                                  
					})                                                                                                                                                                                                                                                        
					.finally(oSheet.destroy);                                                                                                                                                                                                                                 
                                                                                                                                                                                                                                                               
			},                                                                                                                                                                                                                                                          
			////////////////////Table personalization                                                                                                                                                                                                                   
			onPersoButtonPressedRpt: function (oEvent) {                                                                                                                                                                                                                
                                                                                                                                                                                                                                                               
				this._oTPC2.openDialog();                                                                                                                                                                                                                                  
			},                                                                                                                                                                                                                                                          
			//Search on Calims Report                                                                                                                                                                                                                                   
			onRptSearch: function (oEvent) {                                                                                                                                                                                                                            
                                                                                                                                                                                                                                                               
				var value = oEvent.getParameter("query");                                                                                                                                                                                                                  
				value.toUpperCase();                                                                                                                                                                                                                                       
				var oFilter = new sap.ui.model.Filter("Vbeln", sap.ui.model.FilterOperator.Contains, value);                                                                                                                                                               
				var oFilter2 = new sap.ui.model.Filter("Posnr", sap.ui.model.FilterOperator.Contains, value);                                                                                                                                                              
				var oFilter3 = new sap.ui.model.Filter("Ismcopynr", sap.ui.model.FilterOperator.Contains, value);                                                                                                                                                          
				var oFilter4 = new sap.ui.model.Filter("Extwg", sap.ui.model.FilterOperator.Contains, value);                                                                                                                                                              
				var oFilter11 = new sap.ui.model.Filter("Matnr", sap.ui.model.FilterOperator.Contains, value);                                                                                                                                                             
				var oFilter12 = new sap.ui.model.Filter("Arktx", sap.ui.model.FilterOperator.Contains, value);                                                                                                                                                             
				var oFilter13 = new sap.ui.model.Filter("Kvgr1_Text", sap.ui.model.FilterOperator.Contains, value);                                                                                                                                                        
				var oFilter5 = new sap.ui.model.Filter("Kunnr", sap.ui.model.FilterOperator.Contains, value);                                                                                                                                                              
				var oFilter6 = new sap.ui.model.Filter("Kunwe", sap.ui.model.FilterOperator.Contains, value);                                                                                                                                                              
				var oFilter7 = new sap.ui.model.Filter("Lifnr", sap.ui.model.FilterOperator.Contains, value);                                                                                                                                                              
				var oFilter8 = new sap.ui.model.Filter("Erdat", sap.ui.model.FilterOperator.Contains, value);                                                                                                                                                              
				var oFilter9 = new sap.ui.model.Filter("Sub_Count", sap.ui.model.FilterOperator.Contains, value);                                                                                                                                                          
				var oFilter10 = new sap.ui.model.Filter("Bezei", sap.ui.model.FilterOperator.Contains, value);                                                                                                                                                             
				var oFilter14 = new sap.ui.model.Filter("Kwmeng", sap.ui.model.FilterOperator.Contains, value);                                                                                                                                                            
				var oFilter15 = new sap.ui.model.Filter("Ernam", sap.ui.model.FilterOperator.Contains, value);                                                                                                                                                             
				var oFilter16 = new sap.ui.model.Filter("WadatIst", sap.ui.model.FilterOperator.Contains, value);                                                                                                                                                          
				var oFilter17 = new sap.ui.model.Filter("Pstyv", sap.ui.model.FilterOperator.Contains, value);                                                                                                                                                             
				var oFilter18 = new sap.ui.model.Filter("Status", sap.ui.model.FilterOperator.Contains, value);                                                                                                                                                            
				var oFilter19 = new sap.ui.model.Filter("Vgbel", sap.ui.model.FilterOperator.Contains, value);                                                                                                                                                             
				var oFilter20 = new sap.ui.model.Filter("Vgpos", sap.ui.model.FilterOperator.Contains, value);                                                                                                                                                             
				var oFilter21 = new sap.ui.model.Filter("VbelnVl", sap.ui.model.FilterOperator.Contains, value);                                                                                                                                                           
				var oFilter22 = new sap.ui.model.Filter("Banfn", sap.ui.model.FilterOperator.Contains, value);                                                                                                                                                             
				var oFilter23 = new sap.ui.model.Filter("Ebeln", sap.ui.model.FilterOperator.Contains, value);                                                                                                                                                             
				var oFilter24 = new sap.ui.model.Filter("AG_Name1", sap.ui.model.FilterOperator.Contains, value);                                                                                                                                                          
				var oFilter25 = new sap.ui.model.Filter("WE_Name1", sap.ui.model.FilterOperator.Contains, value);                                                                                                                                                          
				var oFilter26 = new sap.ui.model.Filter("AG_Email", sap.ui.model.FilterOperator.Contains, value);                                                                                                                                                          
				var oFilter27 = new sap.ui.model.Filter("AG_Name2", sap.ui.model.FilterOperator.Contains, value);                                                                                                                                                          
				var oFilter28 = new sap.ui.model.Filter("AG_Land1", sap.ui.model.FilterOperator.Contains, value);                                                                                                                                                          
				var oFilter29 = new sap.ui.model.Filter("WE_Name2", sap.ui.model.FilterOperator.Contains, value);                                                                                                                                                          
				var oFilter30 = new sap.ui.model.Filter("WE_Email", sap.ui.model.FilterOperator.Contains, value);                                                                                                                                                          
				var oFilter31 = new sap.ui.model.Filter("WE_Land1", sap.ui.model.FilterOperator.Contains, value);                                                                                                                                                          
				var oFilter32 = new sap.ui.model.Filter("AG_TEL", sap.ui.model.FilterOperator.Contains, value);                                                                                                                                                            
				var oFilter33 = new sap.ui.model.Filter("WE_Tel", sap.ui.model.FilterOperator.Contains, value);                                                                                                                                                            
				var oFilter34 = new sap.ui.model.Filter("WE_Region", sap.ui.model.FilterOperator.Contains, value);                                                                                                                                                         
				var oTable = this.getView().byId("rpt");                                                                                                                                                                                                                   
                                                                                                                                                                                                                                                               
				var oTableItems = oTable.getBinding("rows");                                                                                                                                                                                                               
                                                                                                                                                                                                                                                               
				var orFilter = new sap.ui.model.Filter({                                                                                                                                                                                                                   
					filters: [oFilter, oFilter2, oFilter3, oFilter4, oFilter5, oFilter6, oFilter7, oFilter8, oFilter9, oFilter10,                                                                                                                                             
						oFilter11, oFilter12, oFilter13, oFilter14, oFilter15, oFilter16, oFilter17, oFilter18, oFilter19, oFilter20,                                                                                                                                            
						oFilter20, oFilter21, oFilter22, oFilter23, oFilter24, oFilter25, oFilter26, oFilter27, oFilter28, oFilter29,                                                                                                                                            
						oFilter30, oFilter31, oFilter32, oFilter33, oFilter34                                                                                                                                                                                                    
					],                                                                                                                                                                                                                                                        
					and: false                                                                                                                                                                                                                                                
				});                                                                                                                                                                                                                                                        
				var aFilter = [orFilter];                                                                                                                                                                                                                                  
				oTableItems.filter(aFilter);                                                                                                                                                                                                                               
				var count = oTableItems.getLength();                                                                                                                                                                                                                       
				this.getView().byId("count").setValue(count);                                                                                                                                                                                                              
                                                                                                                                                                                                                                                               
			}                                                                                                                                                                                                                                                           
                                                                                                                                                                                                                                                               
		});                                                                                                                                                                                                                                                          
	});                                                                                                                                                                                                                                                           