				});                                                                                                                                                                                                                                                        
                                                                                                                                                                                                                                                               
				/////set Tokens for multi inputs - Call back function                                                                                                                                                                                                      
				var multiInputValidator = function (input) {                                                                                                                                                                                                               
					var text = input.text;                                                                                                                                                                                                                                    
					return new sap.m.Token({                                                                                                                                                                                                                                  
						key: text,                                                                                                                                                                                                                                               
						text: text                                                                                                                                                                                                                                               
					});                                                                                                                                                                                                                                                       
				};                                                                                                                                                                                                                                                         
                                                                                                                                                                                                                                                               
				var oMultiInput1 = this.getView().byId("multiInput1");                                                                                                                                                                                                     
				oMultiInput1.addValidator(multiInputValidator);                                                                                                                                                                                                            
                                                                                                                                                                                                                                                               
				var oMultiInput2 = this.getView().byId("multiInput2");                                                                                                                                                                                                     
				oMultiInput2.addValidator(multiInputValidator);                                                                                                                                                                                                            
                                                                                                                                                                                                                                                               
				var oMultiInput3 = this.getView().byId("multiInput3");                                                                                                                                                                                                     
				oMultiInput3.addValidator(multiInputValidator);                                                                                                                                                                                                            
                                                                                                                                                                                                                                                               
				var oMultiInput4 = this.getView().byId("multiInput4");                                                                                                                                                                                                     
				oMultiInput4.addValidator(multiInputValidator);                                                                                                                                                                                                            
				////Set default dates for Claims Report                                                                                                                                                                                                                    
				var oDate = new Date();                                                                                                                                                                                                                                    
				var date = oDate;                                                                                                                                                                                                                                          
				var year = 365;                                                                                                                                                                                                                                            
				date = date.setDate(date.getDate() - year);                                                                                                                                                                                                                
				this.getView().byId("IDatefrom").setDateValue(new Date(date));                                                                                                                                                                                             
				var oDate2 = new Date();                                                                                                                                                                                                                                   
				this.getView().byId("IDateto").setDateValue(oDate2);                                                                                                                                                                                                       
			},                                                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
			onAfterRendering: function () {                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                               
				//set default BP                                                                                                                                                                                                                                           
				////this.getView().byId("Ibp").setSelectedKey(sap.ui.getCore().bp);                                                                                                                                                                                        
