					}, {                                                                                                                                                                                                                                                      
						label: "Claim Processed Date",                                                                                                                                                                                                                           
						property: "WadatIst",                                                                                                                                                                                                                                    
						type: EdmType.String,                                                                                                                                                                                                                                    
						width: "10"                                                                                                                                                                                                                                              
					}, {                                                                                                                                                                                                                                                      
						label: "Item Category",                                                                                                                                                                                                                                  
						property: "Vtext",                                                                                                                                                                                                                                       
						type: EdmType.String,                                                                                                                                                                                                                                    
