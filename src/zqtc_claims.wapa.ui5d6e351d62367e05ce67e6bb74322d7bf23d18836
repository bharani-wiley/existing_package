						width: "10"                                                                                                                                                                                                                                              
					}, {                                                                                                                                                                                                                                                      
						label: "Approver",                                                                                                                                                                                                                                       
						property: "Ernam",                                                                                                                                                                                                                                       
						type: EdmType.String,                                                                                                                                                                                                                                    
