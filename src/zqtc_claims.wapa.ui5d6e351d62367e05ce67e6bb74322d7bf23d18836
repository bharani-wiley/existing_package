					});                                                                                                                                                                                                                                                       
				}                                                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
				this.cValueHelpDialog.then(function (dValueHelpDialog) {                                                                                                                                                                                                   
					// create a filter for the binding                                                                                                                                                                                                                        
					// var oFilter = new Filter("LANDX", FilterOperator.Contains, sInputValue);                                                                                                                                                                               
                                                                                                                                                                                                                                                               
