					// ]);                                                                                                                                                                                                                                                    
					// open value help dialog filtered by the input value                                                                                                                                                                                                     
					dValueHelpDialog.open(sInputValue);                                                                                                                                                                                                                       
				});                                                                                                                                                                                                                                                        
			},                                                                                                                                                                                                                                                          
			handleCtryRptSearch: function (evt) {                                                                                                                                                                                                                       
				var sValue = evt.getParameter("value");                                                                                                                                                                                                                    
				//	var oFilter = new Filter("Land1", FilterOperator.Contains, sValue);                                                                                                                                                                                     
				var oFilter = new Filter("Landx", FilterOperator.EQ, sValue);                                                                                                                                                                                              
                                                                                                                                                                                                                                                               
				evt.getSource().getBinding("items").filter([oFilter]);                                                                                                                                                                                                     
			},                                                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
			handleCtryRptClose: function (evt) {                                                                                                                                                                                                                        
				var aSelectedItems = evt.getParameter("selectedItems"),                                                                                                                                                                                                    
					oMultiInput2 = this.getView().byId("multiInput2");                                                                                                                                                                                                        
                                                                                                                                                                                                                                                               
				if (aSelectedItems && aSelectedItems.length > 0) {                                                                                                                                                                                                         
					aSelectedItems.forEach(function (oItem) {                                                                                                                                                                                                                 
						oMultiInput2.addToken(new sap.m.Token({                                                                                                                                                                                                                  
							text: oItem.getTitle()                                                                                                                                                                                                                                  
						}));                                                                                                                                                                                                                                                     
					});                                                                                                                                                                                                                                                       
				}                                                                                                                                                                                                                                                          
			},                                                                                                                                                                                                                                                          
			////SoldTo popup on Claims report                                                                                                                                                                                                                           
			soldToRptTab: function (oEvent) {                                                                                                                                                                                                                           
				var soldPath = oEvent.getSource().getBindingContext("rpt").getPath();                                                                                                                                                                                      
                                                                                                                                                                                                                                                               
				if (!this.fragmentSoldToAdd) {                                                                                                                                                                                                                             
					this.fragmentSoldToAdd = new sap.ui.xmlfragment("zqtc.ZQTC_CLAIMS.view.fragmentSoldtoAddRpt", this);                                                                                                                                                      
					this.getView().addDependent(this.fragmentSoldToAdd);                                                                                                                                                                                                      
				}                                                                                                                                                                                                                                                          
				this.fragmentSoldToAdd.setTitle("SoldTo-Party Address");                                                                                                                                                                                                   
				sap.ui.getCore().byId("SimpleFormAdd1").bindElement({                                                                                                                                                                                                      
					path: soldPath,                                                                                                                                                                                                                                           
					model: "rpt"                                                                                                                                                                                                                                              
				});                                                                                                                                                                                                                                                        
				this.fragmentSoldToAdd.open();                                                                                                                                                                                                                             
			},                                                                                                                                                                                                                                                          
			cancelAdd1: function () {                                                                                                                                                                                                                                   
				this.fragmentSoldToAdd.close();                                                                                                                                                                                                                            
			},                                                                                                                                                                                                                                                          
			/////Shipto pop up on Claims report                                                                                                                                                                                                                         
			shipToRptTab: function (oEvent) {                                                                                                                                                                                                                           
