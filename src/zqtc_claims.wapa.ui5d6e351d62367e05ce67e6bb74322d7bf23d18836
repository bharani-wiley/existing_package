				//check authorization/////////////////////////                                                                                                                                                                                                             
				var that = this;                                                                                                                                                                                                                                           
				this.getOwnerComponent().getModel().read("/ClaimAuthSet", {                                                                                                                                                                                                
					success: function (odata) {                                                                                                                                                                                                                               
						if (odata.results[0].ClaimAll === "X") {                                                                                                                                                                                                                 
							that.getView().byId("claimsProcess").setVisible(true);                                                                                                                                                                                                  
							that.getView().byId("claimsProcess").setIcon("sap-icon://begin");                                                                                                                                                                                       
							that.getView().byId("claimsProcess").setIconColor("Positive");                                                                                                                                                                                          
							that.getView().byId("claimsRpt").setVisible(true);                                                                                                                                                                                                      
							that.getView().byId("claimsRpt").setIcon("sap-icon://expense-report");                                                                                                                                                                                  
							that.getView().byId("claimsRpt").setIconColor("Positive");                                                                                                                                                                                              
						} else if (odata.results[0].ClaimReport === "X") {                                                                                                                                                                                                       
							that.getView().byId("claimsRpt").setVisible(true);                                                                                                                                                                                                      
							that.getView().byId("claimsRpt").setIcon("");                                                                                                                                                                                                           
							that.getView().byId("rpt").setVisibleRowCount(10);                                                                                                                                                                                                      
						} else {                                                                                                                                                                                                                                                 
							that.getView().byId("claimsProcess").setVisible(false);                                                                                                                                                                                                 
							that.getView().byId("claimsRpt").setVisible(false);                                                                                                                                                                                                     
							that.getView().byId("idIconTabBar").setVisible(false);                                                                                                                                                                                                  
							MessageBox.error("No authorization to view the Claims");                                                                                                                                                                                                
						}                                                                                                                                                                                                                                                        
					},                                                                                                                                                                                                                                                        
					error: function (odata) {                                                                                                                                                                                                                                 
						MessageBox.error("No authorization to view the Claims");                                                                                                                                                                                                 
					}                                                                                                                                                                                                                                                         
				});                                                                                                                                                                                                                                                        
				//////Session Timeout implementation                                                                                                                                                                                                                       
				// document.onmousemove = timeOut;                                                                                                                                                                                                                         
				var osetTime;                                                                                                                                                                                                                                              
				var oPromise = new Promise(function (res, rej) {                                                                                                                                                                                                           
					this.getOwnerComponent().getModel().read("/TIMESet", {                                                                                                                                                                                                    
						success: function (data) {                                                                                                                                                                                                                               
							osetTime = data.results[0].Time * 1000;                                                                                                                                                                                                                 
							res();                                                                                                                                                                                                                                                  
						},                                                                                                                                                                                                                                                       
						error: function (err) {                                                                                                                                                                                                                                  
							osetTime = "300000";                                                                                                                                                                                                                                    
							rej();                                                                                                                                                                                                                                                  
						}                                                                                                                                                                                                                                                        
					});                                                                                                                                                                                                                                                       
				}.bind(this));                                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                               
				oPromise.then(function (val) {                                                                                                                                                                                                                             
					var timer;                                                                                                                                                                                                                                                
                                                                                                                                                                                                                                                               
					function timeOut() {                                                                                                                                                                                                                                      
						clearTimeout(timer);                                                                                                                                                                                                                                     
						timer = setTimeout(function () {                                                                                                                                                                                                                         
							MessageBox.show(                                                                                                                                                                                                                                        
								"Click OK to Refresh", {                                                                                                                                                                                                                               
									icon: MessageBox.Icon.Error,                                                                                                                                                                                                                          
									title: "Session Timeout",                                                                                                                                                                                                                             
