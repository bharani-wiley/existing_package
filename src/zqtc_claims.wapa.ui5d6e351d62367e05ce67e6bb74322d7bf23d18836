			},                                                                                                                                                                                                                                                          
			/////Claims Popup for delivery details                                                                                                                                                                                                                      
			claimsRptTab: function (oEvent) {                                                                                                                                                                                                                           
				var issuePath = oEvent.getSource().getBindingContext("rpt").getPath();                                                                                                                                                                                     
				var claim = oEvent.getSource().getBindingContext("rpt").getProperty("Vbeln");                                                                                                                                                                              
				if (!this.fragmentDeladd) {                                                                                                                                                                                                                                
					this.fragmentDeladd = new sap.ui.xmlfragment("zqtc.ZQTC_CLAIMS.view.fragmentDelRpt", this);                                                                                                                                                               
					this.getView().addDependent(this.fragmentDeladd);                                                                                                                                                                                                         
				}                                                                                                                                                                                                                                                          
				this.fragmentDeladd.setTitle("Claim:" + claim + " " + "Delivery Details");                                                                                                                                                                                 
				sap.ui.getCore().byId("SimpleFormAdd5").bindElement({                                                                                                                                                                                                      
					path: issuePath,                                                                                                                                                                                                                                          
					model: "rpt"                                                                                                                                                                                                                                              
				});                                                                                                                                                                                                                                                        
				this.fragmentDeladd.open();                                                                                                                                                                                                                                
			},                                                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
			cancelAdd5: function () {                                                                                                                                                                                                                                   
                                                                                                                                                                                                                                                               
				this.fragmentDeladd.close();                                                                                                                                                                                                                               
                                                                                                                                                                                                                                                               
			},                                                                                                                                                                                                                                                          
			//Fetcch Claims report info                                                                                                                                                                                                                                 
			refreshRpt: function (oEvent) {                                                                                                                                                                                                                             
                                                                                                                                                                                                                                                               
				var createDateFrom = this.getView().byId("IDatefrom").getValue();                                                                                                                                                                                          
				var createDateTo = this.getView().byId("IDateto").getValue();                                                                                                                                                                                              
				if (!createDateFrom) {                                                                                                                                                                                                                                     
					createDateFrom = null;                                                                                                                                                                                                                                    
				}                                                                                                                                                                                                                                                          
				if (!createDateTo) {                                                                                                                                                                                                                                       
					createDateTo = null;                                                                                                                                                                                                                                      
				}                                                                                                                                                                                                                                                          
				var raisedDateFrom = this.getView().byId("IDatefrom2").getValue();                                                                                                                                                                                         
				if (!raisedDateFrom) {                                                                                                                                                                                                                                     
					raisedDateFrom = null;                                                                                                                                                                                                                                    
				}                                                                                                                                                                                                                                                          
				var raisedDateTo = this.getView().byId("IDateto2").getValue();                                                                                                                                                                                             
				if (!raisedDateTo) {                                                                                                                                                                                                                                       
					raisedDateTo = null;                                                                                                                                                                                                                                      
				}                                                                                                                                                                                                                                                          
                                                                                                                                                                                                                                                               
				sap.ui.getCore().enddate = this.getView().byId("IDateto").getValue();                                                                                                                                                                                      
				var tUsers = this.getView().byId("multiInput1").getTokens();                                                                                                                                                                                               
				var tCntry = this.getView().byId("multiInput2").getTokens();                                                                                                                                                                                               
				var tSoldto = this.getView().byId("multiInput3").getTokens();                                                                                                                                                                                              
				var tShipto = this.getView().byId("multiInput4").getTokens();                                                                                                                                                                                              
				var Users = [],                                                                                                                                                                                                                                            
					Cntry = [],                                                                                                                                                                                                                                               
					Soldto = [],                                                                                                                                                                                                                                              
					Shipto = [];                                                                                                                                                                                                                                              
				if (tUsers.length > 0) {                                                                                                                                                                                                                                   
					for (var i = 0; i < tUsers.length; i++) {                                                                                                                                                                                                                 
						var selectedValue = tUsers[i].getProperty("text");                                                                                                                                                                                                       
						Users.push(selectedValue);                                                                                                                                                                                                                               
					}                                                                                                                                                                                                                                                         
				}                                                                                                                                                                                                                                                          
				if (tCntry.length > 0) {                                                                                                                                                                                                                                   
					for (var j = 0; j < tCntry.length; j++) {                                                                                                                                                                                                                 
						var selectedValue2 = tCntry[j].getProperty("text");                                                                                                                                                                                                      
						Cntry.push(selectedValue2);                                                                                                                                                                                                                              
					}                                                                                                                                                                                                                                                         
				}                                                                                                                                                                                                                                                          
				if (tSoldto.length > 0) {                                                                                                                                                                                                                                  
					for (var k = 0; k < tSoldto.length; k++) {                                                                                                                                                                                                                
						var selectedValue3 = tSoldto[k].getProperty("text");                                                                                                                                                                                                     
						Soldto.push(selectedValue3);                                                                                                                                                                                                                             
					}                                                                                                                                                                                                                                                         
				}                                                                                                                                                                                                                                                          
				if (tShipto.length > 0) {                                                                                                                                                                                                                                  
					for (var l = 0; l < tShipto.length; l++) {                                                                                                                                                                                                                
						var selectedValue4 = tShipto[l].getProperty("text");                                                                                                                                                                                                     
						Shipto.push(selectedValue4);                                                                                                                                                                                                                             
					}                                                                                                                                                                                                                                                         
				}                                                                                                                                                                                                                                                          
				var oUsers = new sap.ui.model.Filter("Ernam", sap.ui.model.FilterOperator.EQ, Users);                                                                                                                                                                      
				var oCntrys = new sap.ui.model.Filter("Land1", sap.ui.model.FilterOperator.EQ, Cntry);                                                                                                                                                                     
				var oSoldto = new sap.ui.model.Filter("Kunnr", sap.ui.model.FilterOperator.EQ, Soldto);                                                                                                                                                                    
				var oShipto = new sap.ui.model.Filter("Kunwe", sap.ui.model.FilterOperator.EQ, Shipto);                                                                                                                                                                    
				var oCreateDate = new sap.ui.model.Filter({                                                                                                                                                                                                                
					path: "Erdat",                                                                                                                                                                                                                                            
					operator: sap.ui.model.FilterOperator.BT,                                                                                                                                                                                                                 
					value1: createDateFrom,                                                                                                                                                                                                                                   
					value2: createDateTo                                                                                                                                                                                                                                      
				});                                                                                                                                                                                                                                                        
                                                                                                                                                                                                                                                               
				var oRaisedDate = new sap.ui.model.Filter({                                                                                                                                                                                                                
					path: "WadatIst",                                                                                                                                                                                                                                         
					operator: sap.ui.model.FilterOperator.BT,                                                                                                                                                                                                                 
					value1: raisedDateFrom,                                                                                                                                                                                                                                   
					value2: raisedDateTo                                                                                                                                                                                                                                      
				});                                                                                                                                                                                                                                                        
                                                                                                                                                                                                                                                               
				var oFilter = new sap.ui.model.Filter({                                                                                                                                                                                                                    
					filters: [                                                                                                                                                                                                                                                
						oUsers,                                                                                                                                                                                                                                                  
						oCntrys,                                                                                                                                                                                                                                                 
						oSoldto,                                                                                                                                                                                                                                                 
						oShipto,                                                                                                                                                                                                                                                 
						oCreateDate,                                                                                                                                                                                                                                             
						oRaisedDate                                                                                                                                                                                                                                              
					],                                                                                                                                                                                                                                                        
					and: true                                                                                                                                                                                                                                                 
                                                                                                                                                                                                                                                               
				});                                                                                                                                                                                                                                                        
				/////Get Claims report info                                                                                                                                                                                                                                
				var oDataModel = this.getOwnerComponent().getModel();                                                                                                                                                                                                      
				var that = this;                                                                                                                                                                                                                                           
				that.getView().setBusy(true);                                                                                                                                                                                                                              
				oDataModel.read("/ClaimReportSet", {                                                                                                                                                                                                                       
					filters: [oFilter],                                                                                                                                                                                                                                       
					success: function (oData, oResp) {                                                                                                                                                                                                                        
						that.getView().setBusy(false);                                                                                                                                                                                                                           
						var oJSONModel = new sap.ui.model.json.JSONModel();                                                                                                                                                                                                      
						oJSONModel.setData(oData);                                                                                                                                                                                                                               
						sap.ui.getCore().setModel(oJSONModel);                                                                                                                                                                                                                   
						that.getView().setModel(oJSONModel, "rpt");                                                                                                                                                                                                              
                                                                                                                                                                                                                                                               
						var count = oData.results.length;                                                                                                                                                                                                                        
						that.getView().byId("count").setValue(count);                                                                                                                                                                                                            
						if (count === 0) {                                                                                                                                                                                                                                       
							sap.m.MessageBox.information("No data for the given selection");                                                                                                                                                                                        
						}                                                                                                                                                                                                                                                        
					},                                                                                                                                                                                                                                                        
					error: function (oData) {                                                                                                                                                                                                                                 
						that.getView().setBusy(false);                                                                                                                                                                                                                           
						sap.m.MessageBox.error("No data for the given selection");                                                                                                                                                                                               
					}                                                                                                                                                                                                                                                         
				});                                                                                                                                                                                                                                                        
			},                                                                                                                                                                                                                                                          
			////////////////Download excel on claims Report                                                                                                                                                                                                             
