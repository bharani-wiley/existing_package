<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">                                                                                                                                                                                               
	<ViewSettingsDialog confirm="handleFilterDialogConfirm">                                                                                                                                                                                                      
		<filterItems>                                                                                                                                                                                                                                                
			<ViewSettingsFilterItem text="Age" key="Age" multiSelect="false">                                                                                                                                                                                           
				<items>                                                                                                                                                                                                                                                    
					<ViewSettingsItem text="Less than 1000 days" key="Age___LE___1000___X"/>                                                                                                                                                                                  
					<ViewSettingsItem text="Between 1000 and 2000 days" key="Age___BT___1000___2000"/>                                                                                                                                                                        
					<ViewSettingsItem text="More Than 2000 days" key="Age___GT___2000___X"/>                                                                                                                                                                                  
				</items>                                                                                                                                                                                                                                                   
			</ViewSettingsFilterItem>                                                                                                                                                                                                                                   
			<ViewSettingsFilterItem text="Net Value" key="Value" multiSelect="false">                                                                                                                                                                                   
				<items>                                                                                                                                                                                                                                                    
					<ViewSettingsItem text="Less Than 100" key="Net Value___LE___100___X"/>                                                                                                                                                                                   
					<ViewSettingsItem text="Between 100 and 1000" key="Net Value___BT___100___1000"/>                                                                                                                                                                         
					<ViewSettingsItem text="More Than 1000" key="Net Value___GT___1000___X"/>                                                                                                                                                                                 
				</items>                                                                                                                                                                                                                                                   
			</ViewSettingsFilterItem>                                                                                                                                                                                                                                   
		</filterItems>                                                                                                                                                                                                                                               
	</ViewSettingsDialog>                                                                                                                                                                                                                                         
</core:FragmentDefinition>                                                                                                                                                                                                                                     