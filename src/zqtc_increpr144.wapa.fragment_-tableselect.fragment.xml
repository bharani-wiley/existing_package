<core:FragmentDefinition xmlns:m="sap.m" xmlns="sap.ui.table" xmlns:core="sap.ui.core">                                                                                                                                                                        
	<m:Dialog id="myDialog" title="Table Items">                                                                                                                                                                                                                  
		<Table id="tblItems" rows="{objectView>/showItems/}" selectionMode="None" visibleRowCount="7" paste="onPaste" ariaLabelledBy="title">                                                                                                                        
			<extension>                                                                                                                                                                                                                                                 
				<m:OverflowToolbar style="Clear">                                                                                                                                                                                                                          
					<m:Title id="tblshowtitle" text="Items ({objectView>/showItems/length})"/>                                                                                                                                                                                
					<m:ToolbarSpacer/>                                                                                                                                                                                                                                        
					<m:CheckBox id="chktableSelect" text="Show Rejected Line Items" select="onCheckBoxSelect"></m:CheckBox>                                                                                                                                                   
					<m:SearchField placeholder="Filter" search="tableSelectFilter" width="15rem"/>                                                                                                                                                                            
				</m:OverflowToolbar>                                                                                                                                                                                                                                       
			</extension>                                                                                                                                                                                                                                                
			<columns>                                                                                                                                                                                                                                                   
				<Column width="7rem" sortProperty="POSNR" filterProperty="POSNR">                                                                                                                                                                                          
					<m:Label text="Order"/>                                                                                                                                                                                                                                   
					<template>                                                                                                                                                                                                                                                
						<m:Link text="{objectView>VBELN}" href="{objectView>LINKREF}" target="_blank"/>                                                                                                                                                                          
					</template>                                                                                                                                                                                                                                               
				</Column>                                                                                                                                                                                                                                                  
				<Column width="5rem" sortProperty="POSNR" filterProperty="POSNR">                                                                                                                                                                                          
					<m:Label text="Line Item"/>                                                                                                                                                                                                                               
					<template>                                                                                                                                                                                                                                                
						<m:Label text="{objectView>POSNR}"/>                                                                                                                                                                                                                     
					</template>                                                                                                                                                                                                                                               
				</Column>                                                                                                                                                                                                                                                  
				<Column visible="{=${objectView>/showItems/0/MISSING}.length ? true : false}" width="7rem" sortProperty="MISSING" filterProperty="MISSING" autoResizable="true">                                                                                           
					<m:Label text="Incomp Status"/>                                                                                                                                                                                                                           
					<template>                                                                                                                                                                                                                                                
						<m:Text text="{objectView>MISSING}"/>                                                                                                                                                                                                                    
					</template>                                                                                                                                                                                                                                               
				</Column>                                                                                                                                                                                                                                                  
				<Column width="6rem" hAlign="End" sortProperty="VKUESCH" filterProperty="VKUESCH" autoResizable="true">                                                                                                                                                    
					<m:Label text="Cancellation Procedure" wrapping="true"/>                                                                                                                                                                                                  
					<template>                                                                                                                                                                                                                                                
						<m:Text text="{objectView>VKUESCH}"/>                                                                                                                                                                                                                    
					</template>                                                                                                                                                                                                                                               
				</Column>                                                                                                                                                                                                                                                  
				<Column width="10rem" sortProperty="VKUEGRU" filterProperty="VKUEGRU" autoResizable="true">                                                                                                                                                                
					<m:Label text="Cancellation reason" wrapping="true"/>                                                                                                                                                                                                     
					<template>                                                                                                                                                                                                                                                
						<m:Label text="{objectView>VKUEGRU}"/>                                                                                                                                                                                                                   
					</template>                                                                                                                                                                                                                                               
				</Column>                                                                                                                                                                                                                                                  
				<Column width="7rem" sortProperty="VBEDKUE" filterProperty="VBEDKUE" autoResizable="true">                                                                                                                                                                 
					<m:Label text="Cancellation Date" wrapping="true"/>                                                                                                                                                                                                       
					<template>                                                                                                                                                                                                                                                
						<m:Label text="{ path: 'objectView>VBEDKUE', type: 'sap.ui.model.type.Date', formatOptions: {source: {pattern: 'dd/MM/yyyy'}} }"/>                                                                                                                       
						<!--<u:Currency value="{Price}" currency="{CurrencyCode}"/>-->                                                                                                                                                                                           
					</template>                                                                                                                                                                                                                                               
				</Column>                                                                                                                                                                                                                                                  
				<Column width="10rem" sortProperty="ABGRU" filterProperty="ABGRU" autoResizable="true">                                                                                                                                                                    
					<m:Label text="Rejection Reason" wrapping="true"/>                                                                                                                                                                                                        
					<template>                                                                                                                                                                                                                                                
						<m:Label text="{objectView>ABGRU}"/>                                                                                                                                                                                                                     
					</template>                                                                                                                                                                                                                                               
				</Column>                                                                                                                                                                                                                                                  
				<Column width="7rem" sortProperty="VBEGDAT" filterProperty="VBEGDAT" autoResizable="true">                                                                                                                                                                 
					<m:Label text="Contract Start Date" wrapping="true"/>                                                                                                                                                                                                     
					<template>                                                                                                                                                                                                                                                
						<m:Label text="{ path: 'objectView>VBEGDAT', type: 'sap.ui.model.type.Date', formatOptions: {source: {pattern: 'dd/MM/yyyy'}} }"/>                                                                                                                       
					</template>                                                                                                                                                                                                                                               
				</Column>                                                                                                                                                                                                                                                  
				<Column width="7rem" sortProperty="VENDDAT" filterProperty="VENDDAT" autoResizable="true">                                                                                                                                                                 
					<m:Label text="Contract End Date" wrapping="true"/>                                                                                                                                                                                                       
					<template>                                                                                                                                                                                                                                                
						<m:Label text="{ path: 'objectView>VENDDAT', type: 'sap.ui.model.type.Date', formatOptions: {source: {pattern: 'dd/MM/yyyy'}} }"/>                                                                                                                       
					</template>                                                                                                                                                                                                                                               
				</Column>                                                                                                                                                                                                                                                  
				<Column width="12rem" sortProperty="MATNR" filterProperty="MATNR" autoResizable="true">                                                                                                                                                                    
					<m:Label text="Material"/>                                                                                                                                                                                                                                
					<template>                                                                                                                                                                                                                                                
						<m:Label text="{objectView>MATNR}"/>                                                                                                                                                                                                                     
					</template>                                                                                                                                                                                                                                               
				</Column>                                                                                                                                                                                                                                                  
				<Column width="15rem" sortProperty="MAKTX" filterProperty="MAKTX" autoResizable="true">                                                                                                                                                                    
					<m:Label text="Material Description"/>                                                                                                                                                                                                                    
					<template>                                                                                                                                                                                                                                                
						<m:Label text="{objectView>MAKTX}" wrapping="true"/>                                                                                                                                                                                                     
					</template>                                                                                                                                                                                                                                               
				</Column>                                                                                                                                                                                                                                                  
				<Column width="5rem" sortProperty="ZMENG" filterProperty="ZMENG" autoResizable="true">                                                                                                                                                                     
					<m:Label text="Target Qty" wrapping="true"/>                                                                                                                                                                                                              
					<template>                                                                                                                                                                                                                                                
						<m:Label text="{objectView>ZMENG}"/>                                                                                                                                                                                                                     
					</template>                                                                                                                                                                                                                                               
				</Column>                                                                                                                                                                                                                                                  
				<Column width="5rem" hAlign="Center" sortProperty="NETWR" filterProperty="NETWR" autoResizable="true">                                                                                                                                                     
					<m:Label text="Item Value" wrapping="true"/>                                                                                                                                                                                                              
					<template>                                                                                                                                                                                                                                                
						<m:Label text="{objectView>NETWR}"/>                                                                                                                                                                                                                     
					</template>                                                                                                                                                                                                                                               
				</Column>                                                                                                                                                                                                                                                  
				<Column width="5rem" hAlign="Center" sortProperty="MWSBP" filterProperty="MWSBP" autoResizable="true">                                                                                                                                                     
					<m:Label text="Tax Value" wrapping="true"/>                                                                                                                                                                                                               
					<template>                                                                                                                                                                                                                                                
						<m:Label text="{objectView>MWSBP}"/>                                                                                                                                                                                                                     
					</template>                                                                                                                                                                                                                                               
				</Column>                                                                                                                                                                                                                                                  
				<!---->                                                                                                                                                                                                                                                    
				<Column width="5rem" hAlign="Center" sortProperty="WAERK" filterProperty="WAERK" autoResizable="true">                                                                                                                                                     
					<m:Label text="Order Currency" wrapping="true"/>                                                                                                                                                                                                          
					<template>                                                                                                                                                                                                                                                
						<m:Label text="{objectView>WAERK}"/>                                                                                                                                                                                                                     
					</template>                                                                                                                                                                                                                                               
				</Column>                                                                                                                                                                                                                                                  
				<Column width="7rem" hAlign="Center" sortProperty="FKDAT" filterProperty="FKDAT" autoResizable="true">                                                                                                                                                     
					<m:Label text="Billing Date" wrapping="true"/>                                                                                                                                                                                                            
					<template>                                                                                                                                                                                                                                                
						<m:Label text="{ path: 'objectView>FKDAT', type: 'sap.ui.model.type.Date', formatOptions: {source: {pattern: 'dd/MM/yyyy'}} }"/>                                                                                                                         
					</template>                                                                                                                                                                                                                                               
				</Column>                                                                                                                                                                                                                                                  
				<Column width="12rem" hAlign="Center" sortProperty="ISSUESSENT" filterProperty="ISSUESSENT" autoResizable="true">                                                                                                                                          
					<m:Label text="Issues Sent"/>                                                                                                                                                                                                                             
					<template>                                                                                                                                                                                                                                                
						<m:Label text="{objectView>ISSUESSENT}"/>                                                                                                                                                                                                                
					</template>                                                                                                                                                                                                                                               
				</Column>                                                                                                                                                                                                                                                  
				<Column width="8rem" hAlign="Center" sortProperty="ISSUESPEND" filterProperty="ISSUESPEND" autoResizable="true">                                                                                                                                           
					<m:Label text="Issues Pending"/>                                                                                                                                                                                                                          
					<template>                                                                                                                                                                                                                                                
						<m:Label text="{objectView>ISSUESPEND}"/>                                                                                                                                                                                                                
					</template>                                                                                                                                                                                                                                               
				</Column>                                                                                                                                                                                                                                                  
				<Column width="12rem" hAlign="Center" sortProperty="MVGR5" filterProperty="MVGR5" autoResizable="true">                                                                                                                                                    
					<m:Label text="Material Group 5"/>                                                                                                                                                                                                                        
					<template>                                                                                                                                                                                                                                                
						<m:Label text="{objectView>MVGR5}"/>                                                                                                                                                                                                                     
					</template>                                                                                                                                                                                                                                               
				</Column>                                                                                                                                                                                                                                                  
				<Column width="8rem" hAlign="Center" sortProperty="ZABP" filterProperty="ZABP" autoResizable="true">                                                                                                                                                       
					<m:Label text="Volume Year"/>                                                                                                                                                                                                                             
					<template>                                                                                                                                                                                                                                                
						<m:Label text="{objectView>ZZVYP}"/>                                                                                                                                                                                                                     
					</template>                                                                                                                                                                                                                                               
				</Column>                                                                                                                                                                                                                                                  
				<Column width="8rem" hAlign="Center" sortProperty="KDKG2" filterProperty="KDKG2" autoResizable="true">                                                                                                                                                     
					<m:Label text="Condition Group 2" wrapping="true"/>                                                                                                                                                                                                       
					<template>                                                                                                                                                                                                                                                
						<m:Label text="{objectView>KDKG2}"/>                                                                                                                                                                                                                     
					</template>                                                                                                                                                                                                                                               
				</Column>                                                                                                                                                                                                                                                  
				<Column width="8rem" hAlign="Center" sortProperty="KONDA" filterProperty="KONDA" autoResizable="true">                                                                                                                                                     
					<m:Label text="Price Group"/>                                                                                                                                                                                                                             
					<template>                                                                                                                                                                                                                                                
						<m:Label text="{objectView>KONDA}"/>                                                                                                                                                                                                                     
					</template>                                                                                                                                                                                                                                               
				</Column>                                                                                                                                                                                                                                                  
				<Column width="8rem" hAlign="Center" sortProperty="GBSTA" filterProperty="GBSTA" autoResizable="true">                                                                                                                                                     
					<m:Label text="Overall Status Item" wrapping="true"/>                                                                                                                                                                                                     
					<template>                                                                                                                                                                                                                                                
						<m:Label text="{objectView>GBSTA}"/>                                                                                                                                                                                                                     
					</template>                                                                                                                                                                                                                                               
				</Column>                                                                                                                                                                                                                                                  
				<Column width="8rem" hAlign="Center" sortProperty="CMGST" filterProperty="CMGST" autoResizable="true">                                                                                                                                                     
					<m:Label text="Credit Status"/>                                                                                                                                                                                                                           
					<template>                                                                                                                                                                                                                                                
						<m:Label text="{objectView>CMGST}"/>                                                                                                                                                                                                                     
					</template>                                                                                                                                                                                                                                               
				</Column>                                                                                                                                                                                                                                                  
				<Column width="8rem" hAlign="Center" sortProperty="PSTATUS" filterProperty="PSTATUS" autoResizable="true">                                                                                                                                                 
					<m:Label text="Paid/Not Paid"/>                                                                                                                                                                                                                           
					<template>                                                                                                                                                                                                                                                
						<m:Label text="{objectView>PSTATUS}"/>                                                                                                                                                                                                                   
					</template>                                                                                                                                                                                                                                               
				</Column>                                                                                                                                                                                                                                                  
				<Column width="8rem" hAlign="Center" sortProperty="SHPBP" filterProperty="SHPBP" autoResizable="true">                                                                                                                                                     
					<m:Label text="Ship To BP"/>                                                                                                                                                                                                                              
					<template>                                                                                                                                                                                                                                                
						<m:Label text="{objectView>SHPBP}"/>                                                                                                                                                                                                                     
					</template>                                                                                                                                                                                                                                               
				</Column>                                                                                                                                                                                                                                                  
				<Column width="8rem" hAlign="Center" sortProperty="SHPNAME" filterProperty="SHPNAME" autoResizable="true">                                                                                                                                                 
					<m:Label text="Ship To Name"/>                                                                                                                                                                                                                            
					<template>                                                                                                                                                                                                                                                
						<m:Label text="{objectView>SHPNAME}" wrapping="true"/>                                                                                                                                                                                                   
					</template>                                                                                                                                                                                                                                               
				</Column>                                                                                                                                                                                                                                                  
				<Column width="8rem" hAlign="Center" sortProperty="ZZSUBTYP" filterProperty="ZZSUBTYP" autoResizable="true">                                                                                                                                               
					<m:Label text="Subscription Type" wrapping="true"/>                                                                                                                                                                                                       
					<template>                                                                                                                                                                                                                                                
						<m:Label text="{objectView>ZZSUBTYP}"/>                                                                                                                                                                                                                  
					</template>                                                                                                                                                                                                                                               
				</Column>                                                                                                                                                                                                                                                  
			</columns>                                                                                                                                                                                                                                                  
			<!--<footer>-->                                                                                                                                                                                                                                             
			<!--	<m:OverflowToolbar id="infobar">-->                                                                                                                                                                                                                    
			<!--		<m:Button text="Excel"></m:Button>-->                                                                                                                                                                                                                 
			<!--	</m:OverflowToolbar>-->                                                                                                                                                                                                                                
			<!--</footer>-->                                                                                                                                                                                                                                            
		</Table>                                                                                                                                                                                                                                                     
		<m:endButton>                                                                                                                                                                                                                                                
			<m:Button text="Close" press="onClosetblItems"></m:Button>                                                                                                                                                                                                  
		</m:endButton>                                                                                                                                                                                                                                               
	</m:Dialog>                                                                                                                                                                                                                                                   
</core:FragmentDefinition>                                                                                                                                                                                                                                     