<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">                                                                                                                                                                                               
	<Dialog resizable="true" title="Released Documents">                                                                                                                                                                                                          
		<content>                                                                                                                                                                                                                                                    
			<List items="{mModel>/}" id="dlgListMessageModel">                                                                                                                                                                                                          
				<DisplayListItem label="{mModel>Vbeln}"/>                                                                                                                                                                                                                  
			</List>                                                                                                                                                                                                                                                     
		</content>                                                                                                                                                                                                                                                   
		<beginButton>                                                                                                                                                                                                                                                
			<Button press="onMessageDlgClose" text="Close"></Button>                                                                                                                                                                                                    
		</beginButton>                                                                                                                                                                                                                                               
		<endButton>                                                                                                                                                                                                                                                  
			<Button press="downlaodRelDocs" text="Export"></Button>                                                                                                                                                                                                     
		</endButton>                                                                                                                                                                                                                                                 
	</Dialog>                                                                                                                                                                                                                                                     
	<!--<MessagePopover items="{mModel>/}" initiallyExpanded="true" >-->                                                                                                                                                                                          
	<!--	<MessagePopoverItem title="{mModel>Vbeln}" type="Success"/>-->                                                                                                                                                                                           
	<!--</MessagePopover>-->                                                                                                                                                                                                                                      
</core:FragmentDefinition>                                                                                                                                                                                                                                     