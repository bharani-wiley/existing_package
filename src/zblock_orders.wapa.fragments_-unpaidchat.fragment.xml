<core:FragmentDefinition xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:semantic="sap.f.semantic" xmlns:core="sap.ui.core"                                                                                                                                    
	xmlns:table="sap.ui.table" xmlns:vizFeeds="sap.viz.ui5.controls.common.feeds" xmlns:commons="sap.suite.ui.commons"                                                                                                                                            
	xmlns:layout="sap.ui.layout" xmlns:viz.data="sap.viz.ui5.data" xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds"                                                                                                                                            
	xmlns:vm="sap.ui.comp.variants" xmlns:viz="sap.viz.ui5.controls" xmlns:fb="sap.ui.comp.filterbar">                                                                                                                                                            
	<Panel id="panel" class="styleclass4" expandable="true" expanded="false" expand="panelText" headerText="List Of Unpaid Invoices">                                                                                                                             
		<commons:ChartContainerContent icon="sap-icon://line-chart" title="Chart">                                                                                                                                                                                   
			<commons:content>                                                                                                                                                                                                                                           
				<!--<content>-->                                                                                                                                                                                                                                           
					<viz:Popover id="idPopOver"></viz:Popover>                                                                                                                                                                                                                
					<viz:VizFrame width="100%" id="idVizFrameo" vizType="stacked_bar" uiConfig="{applicationSet:'fiori'}" selectData="onSelectData"                                                                                                                           
						vizProperties="{dataLabel:{ showTotal: true} ,title:{ text:''}}">                                                                                                                                                                                        
						<viz:dataset >                                                                                                                                                                                                                                           
							<viz.data:FlattenedDataset id="idVizDataSet" busy="true" data="{path:'Dunning1>/results'}">                                                                                                                                                             
								<viz.data:dimensions>                                                                                                                                                                                                                                  
									<viz.data:DimensionDefinition name="Year/Month" value="{Dunning1>Date}"/>                                                                                                                                                                             
								</viz.data:dimensions>                                                                                                                                                                                                                                 
								<viz.data:measures>                                                                                                                                                                                                                                    
									<viz.data:MeasureDefinition name="Invoice Count" value="{Dunning1>Count}"/>                                                                                                                                                                           
								</viz.data:measures>                                                                                                                                                                                                                                   
							</viz.data:FlattenedDataset>                                                                                                                                                                                                                            
						</viz:dataset>                                                                                                                                                                                                                                           
						<viz:feeds>                                                                                                                                                                                                                                              
							<viz.feeds:FeedItem id='valueAxisFeed' uid="valueAxis" type="Measure" values="Invoice Count"/>                                                                                                                                                          
							<viz.feeds:FeedItem uid="categoryAxis" type="Dimension" values="Year/Month"/>                                                                                                                                                                           
						</viz:feeds>                                                                                                                                                                                                                                             
					</viz:VizFrame>                                                                                                                                                                                                                                           
				<!--</content>-->                                                                                                                                                                                                                                          
			</commons:content>                                                                                                                                                                                                                                          
		</commons:ChartContainerContent>                                                                                                                                                                                                                             
	</Panel>                                                                                                                                                                                                                                                      
</core:FragmentDefinition>                                                                                                                                                                                                                                     