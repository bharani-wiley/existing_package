<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout"                                                                                                                                           
	xmlns:d="sap.ui.core.LayoutData" xmlns:u="sap.ui.unified">                                                                                                                                                                                                    
	<Dialog resizable="true" draggable="true" id="addDocDialog2" contentWidth="850px" title="Address Suggestions">                                                                                                                                                
		<content >                                                                                                                                                                                                                                                   
			<Table items="{doc2>/results}" id="addDocTable2" growing="true" growingThreshold="10" mode="SingleSelectMaster"                                                                                                                                             
			includeItemInSelection="true" selectionChange="addDocSelectSold">                                                                                                                                                                                           
				<items>                                                                                                                                                                                                                                                    
					<ColumnListItem>                                                                                                                                                                                                                                          
						<cells>                                                                                                                                                                                                                                                  
							<Text text="{doc2>Partner}"/>                                                                                                                                                                                                                           
							<Text text="{doc2>FirstName}"/>                                                                                                                                                                                                                         
							<Text text="{doc2>LastName}"/>                                                                                                                                                                                                                          
						    <Text text="{doc2>City}"/>                                                                                                                                                                                                                           
							<Text text="{doc2>PostalCode}"/>                                                                                                                                                                                                                        
							<Text text="{doc2>Street}"/>                                                                                                                                                                                                                            
							<Text text="{doc2>StrSuppl1}"/>                                                                                                                                                                                                                         
							<Text text="{doc2>StrSuppl2}"/>                                                                                                                                                                                                                         
							<Text text="{doc2>StrSuppl3}"/>                                                                                                                                                                                                                         
						    <Text text="{doc2>Location}"/>                                                                                                                                                                                                                       
							<Text text="{doc2>Streetcode}"/>                                                                                                                                                                                                                        
							<Text text="{doc2>HouseNum1}"/>                                                                                                                                                                                                                         
							<Text text="{doc2>Country}"/>                                                                                                                                                                                                                           
						    <Text text="{doc2>Region}"/>                                                                                                                                                                                                                         
							<Text text="{doc2>CountryName}"/>                                                                                                                                                                                                                       
							<Text text="{doc2>Ecid}"/>                                                                                                                                                                                                                              
						</cells>                                                                                                                                                                                                                                                 
					</ColumnListItem>                                                                                                                                                                                                                                         
				</items>                                                                                                                                                                                                                                                   
				<columns>                                                                                                                                                                                                                                                  
					<Column width="100px">                                                                                                                                                                                                                                    
						<Label text="Partner"/>                                                                                                                                                                                                                                  
					</Column>                                                                                                                                                                                                                                                 
					<Column width="200px">                                                                                                                                                                                                                                    
						<Label text="First Name"/>                                                                                                                                                                                                                               
					</Column>                                                                                                                                                                                                                                                 
					<Column width="100px">                                                                                                                                                                                                                                    
						<Label text="Last Name"/>                                                                                                                                                                                                                                
					</Column>                                                                                                                                                                                                                                                 
					                                                                                                                                                                                                                                                          
					<Column width="70px">                                                                                                                                                                                                                                     
						<Label text="City"/>                                                                                                                                                                                                                                     
					</Column>                                                                                                                                                                                                                                                 
					<Column width="100px">                                                                                                                                                                                                                                    
						<Label text="Postal Code"/>                                                                                                                                                                                                                              
					</Column>                                                                                                                                                                                                                                                 
					<Column width="200px">                                                                                                                                                                                                                                    
						<Label text="Street"/>                                                                                                                                                                                                                                   
					</Column>                                                                                                                                                                                                                                                 
					                                                                                                                                                                                                                                                          
					<Column width="70px">                                                                                                                                                                                                                                     
						<Label text="Street2"/>                                                                                                                                                                                                                                  
					</Column>                                                                                                                                                                                                                                                 
					<Column width="70px">                                                                                                                                                                                                                                     
						<Label text="Street3"/>                                                                                                                                                                                                                                  
					</Column>                                                                                                                                                                                                                                                 
					<Column width="70px">                                                                                                                                                                                                                                     
						<Label text="Street4"/>                                                                                                                                                                                                                                  
					</Column>                                                                                                                                                                                                                                                 
					<Column width="70px">                                                                                                                                                                                                                                     
						<Label text="Location"/>                                                                                                                                                                                                                                 
					</Column>                                                                                                                                                                                                                                                 
					<Column width="50px">                                                                                                                                                                                                                                     
						<Label text="Street Code"/>                                                                                                                                                                                                                              
					</Column>                                                                                                                                                                                                                                                 
					<Column width="50px">                                                                                                                                                                                                                                     
						<Label text="House Num"/>                                                                                                                                                                                                                                
					</Column>                                                                                                                                                                                                                                                 
					<Column width="60px">                                                                                                                                                                                                                                     
						<Label text="Country"/>                                                                                                                                                                                                                                  
					</Column>                                                                                                                                                                                                                                                 
					<Column width="70px">                                                                                                                                                                                                                                     
						<Label text="Region"/>                                                                                                                                                                                                                                   
					</Column>                                                                                                                                                                                                                                                 
					<Column width="100px">                                                                                                                                                                                                                                    
						<Label text="Country Name"/>                                                                                                                                                                                                                             
					</Column>                                                                                                                                                                                                                                                 
					<Column width="100px">                                                                                                                                                                                                                                    
						<Label text="Ecid"/>                                                                                                                                                                                                                                     
					</Column>                                                                                                                                                                                                                                                 
				</columns>                                                                                                                                                                                                                                                 
			</Table>                                                                                                                                                                                                                                                    
		                                                                                                                                                                                                                                                             
	                                                                                                                                                                                                                                                              
		</content>                                                                                                                                                                                                                                                   
		<endButton text="test" height="10px">                                                                                                                                                                                                                        
			<Button icon="sap-icon://sys-cancel" width ="10px" press="cancelAddDocSold" type="Reject" tooltip="close"/>                                                                                                                                                 
		</endButton>                                                                                                                                                                                                                                                 
	</Dialog>                                                                                                                                                                                                                                                     
</core:FragmentDefinition>                                                                                                                                                                                                                                     