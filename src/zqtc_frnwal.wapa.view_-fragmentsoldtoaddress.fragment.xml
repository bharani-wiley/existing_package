<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout"                                                                                                                                           
	xmlns:d="sap.ui.core.LayoutData" xmlns:u="sap.ui.unified">                                                                                                                                                                                                    
	<Dialog resizable="true" draggable="true" id="addDialogSoldTo" contentWidth="650px">                                                                                                                                                                          
		<content >                                                                                                                                                                                                                                                   
			<l:Splitter  orientation="Horizontal">&nbsp;                                                                                                                                                                                                                
				<f:SimpleForm id="SimpleFormSold" editable="false" layout="ResponsiveGridLayout" title="Current Address">                                                                                                                                                  
								<f:content>                                                                                                                                                                                                                                            
								                                                                                                                                                                                                                                                       
									<Label text="Street" design="Bold"/>                                                                                                                                                                                                                  
									<Text text="" width="180px" id="streetSold"/>                                                                                                                                                                                                         
									<Label text="Street2" design="Bold"/>                                                                                                                                                                                                                 
									<Text text="" width="180px" id="street2Sold"/>                                                                                                                                                                                                        
									<Label text="Street3" design="Bold"/>                                                                                                                                                                                                                 
									<Text text="" width="180px" id="street3Sold"/>                                                                                                                                                                                                        
									<Label text="Street4" design="Bold"/>                                                                                                                                                                                                                 
									<Text text="" width="180px" id="street4Sold"/>                                                                                                                                                                                                        
									<Label text="Location/Street5" design="Bold"/>                                                                                                                                                                                                        
									<Text text="" width="180px" id="street5Sold"/>                                                                                                                                                                                                        
									<Label text="City" design="Bold"/>                                                                                                                                                                                                                    
									<Text text=" " width="150px" id="citySold"/>                                                                                                                                                                                                          
									<Label text="Country" design="Bold"  />                                                                                                                                                                                                               
									<Text text=" " width="70px"  id="countrySold"/>                                                                                                                                                                                                       
									<Label text="Region" design="Bold" />                                                                                                                                                                                                                 
									<Text text=" " width="70px" id="regioSold"/>                                                                                                                                                                                                          
									<Label text="Postal Code" design="Bold"/>                                                                                                                                                                                                             
									<Text text=" " width="120px" id="postcodeSold"/>                                                                                                                                                                                                      
								</f:content>                                                                                                                                                                                                                                           
							</f:SimpleForm>                                                                                                                                                                                                                                         
							                                                                                                                                                                                                                                                        
								<f:SimpleForm id="SimpleFormAddSold2" editable="true" layout="ResponsiveGridLayout"                                                                                                                                                                    
								title="New Address">                                                                                                                                                                                                                                   
								<f:content>                                                                                                                                                                                                                                            
								                                                                                                                                                                                                                                                       
									<Label text="Street" design="Bold"/>                                                                                                                                                                                                                  
									<Input value="{AG_Street}" width="250px" class="height" id="streetNewSold"/>                                                                                                                                                                          
									                                                                                                                                                                                                                                                      
									<Label text="Street2" design="Bold"/>                                                                                                                                                                                                                 
									<Input value="{AG_StrSuppl1}" width="250px" class="height" id="street2NewSold"/>                                                                                                                                                                      
									                                                                                                                                                                                                                                                      
									<Label text="Street3" design="Bold"/>                                                                                                                                                                                                                 
									<Input value="{AG_StrSuppl2}" width="250px" class="height" id="street3NewSold"/>                                                                                                                                                                      
									                                                                                                                                                                                                                                                      
									<Label text="Street4" design="Bold"/>                                                                                                                                                                                                                 
									<Input value="{AG_StrSuppl3}" width="250px" class="height" id="street4NewSold"/>                                                                                                                                                                      
									                                                                                                                                                                                                                                                      
									<Label text="Location/Street5" design="Bold"/>                                                                                                                                                                                                        
									<Input value="{AG_Location}" width="250px" class="height" id="locNewSold"/>                                                                                                                                                                           
									                                                                                                                                                                                                                                                      
									<Label text="City" design="Bold"/>                                                                                                                                                                                                                    
									<Input value="{AG_City1}" width="250px" class="height" id="cityNewSold"/>                                                                                                                                                                             
								                                                                                                                                                                                                                                                       
									<HBox>                                                                                                                                                                                                                                                
									<Label text="Country" design="Bold"  class="align"/>                                                                                                                                                                                                  
									<Input value="{AG_Country}" width="80px" showValueHelp="true" id="cntryNewSold" class="height"                                                                                                                                                        
									valueHelpRequest="countryOnF4" valueHelpOnly="true" />                                                                                                                                                                                                
								    <Text text=" " width="10px"/>                                                                                                                                                                                                                      
									<Label text="Region" design="Bold" class="align"/>                                                                                                                                                                                                    
									<Input value="{AG_Region}" width="70px" class="height" showValueHelp="true"&nbsp;                                                                                                                                                                     
									id="regionNewSold" valueHelpRequest="regionOnF4" valueHelpOnly="true"/>                                                                                                                                                                               
									</HBox>                                                                                                                                                                                                                                               
									<HBox>                                                                                                                                                                                                                                                
									<Label text="Postal Code" design="Bold" class="align"/>                                                                                                                                                                                               
									<Input value="{AG_PostCode1}" width="120px" class="height"/>                                                                                                                                                                                          
									</HBox>                                                                                                                                                                                                                                               
									<HBox>                                                                                                                                                                                                                                                
										<Button width="75px" press="addUpdateSold" text="Update" type="Transparent"/>                                                                                                                                                                        
										<Text text=" "/>                                                                                                                                                                                                                                     
										<Button width="75px" press="addResetSold" text="Reset" type="Transparent"/>                                                                                                                                                                          
									</HBox>                                                                                                                                                                                                                                               
								</f:content>                                                                                                                                                                                                                                           
							</f:SimpleForm>                                                                                                                                                                                                                                         
				</l:Splitter>&nbsp;                                                                                                                                                                                                                                        
		                                                                                                                                                                                                                                                             
	                                                                                                                                                                                                                                                              
		</content>                                                                                                                                                                                                                                                   
		<endButton text="test" height="10px">                                                                                                                                                                                                                        
			<Button icon="sap-icon://sys-cancel" width ="10px" press="cancelAddSold" type="Reject" tooltip="close"/>                                                                                                                                                    
		</endButton>                                                                                                                                                                                                                                                 
	</Dialog>                                                                                                                                                                                                                                                     
</core:FragmentDefinition>                                                                                                                                                                                                                                     