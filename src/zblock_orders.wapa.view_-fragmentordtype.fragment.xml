<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" controllerName="Block_Orders.Block_Orders.controller.Worklist">                                                                                                                                
	<Dialog id="sh20" resizable="true" draggable="true" contentWidth="500px">                                                                                                                                                                                     
		<content>                                                                                                                                                                                                                                                    
			<Toolbar id="sh21">                                                                                                                                                                                                                                         
				<OverflowToolbar id="sh22">                                                                                                                                                                                                                                
					<SearchField id="mytypSearch" search="searchEHT" width="250px"/>                                                                                                                                                                                          
				</OverflowToolbar>                                                                                                                                                                                                                                         
				<ToolbarSpacer id="sh23"/>                                                                                                                                                                                                                                 
				<Button id="sh24" icon="sap-icon://clear-filter" press="clearBT" type="Accept" tooltip="Clear Selected Types" width="80px"/>                                                                                                                               
			</Toolbar>                                                                                                                                                                                                                                                  
				<Panel id="BTPanel" expandable="true" expanded="true" headerText="Selected Items">                                                                                                                                                                         
				<Table items="{BTsel>/}" id="BTselTable" growing="true" mode="Delete"                                                                                                                                                                                      
				delete="deleteSelectionBT" showNoData="false" >                                                                                                                                                                                                            
					<items>                                                                                                                                                                                                                                                   
						<ColumnListItem id="sh25">                                                                                                                                                                                                                               
							<cells>                                                                                                                                                                                                                                                 
								<Text id="sh26" text="{BTsel>Auart}"/>                                                                                                                                                                                                                 
							</cells>                                                                                                                                                                                                                                                
						</ColumnListItem>                                                                                                                                                                                                                                        
						</items>                                                                                                                                                                                                                                                 
						<columns>                                                                                                                                                                                                                                                
							<Column id="sh27" width="150px">                                                                                                                                                                                                                        
								<!--<Text text="{BKsel>Kunnr}"/>-->                                                                                                                                                                                                                    
							</Column>                                                                                                                                                                                                                                               
						</columns>                                                                                                                                                                                                                                               
					</Table>                                                                                                                                                                                                                                                  
				</Panel>                                                                                                                                                                                                                                                   
			<Table items="{/OrderTypeSet}" id="BTTable" growing="true" growingThreshold="75" mode="MultiSelect"                                                                                                                                                         
			       	selectionChange="rowSelectionBT" >                                                                                                                                                                                                                  
				<items>                                                                                                                                                                                                                                                    
					<ColumnListItem id="sh28">                                                                                                                                                                                                                                
						<cells>                                                                                                                                                                                                                                                  
							<Text id="ott1" text="{Auart}"/>                                                                                                                                                                                                                        
								<Text id="ott2" text="{Bezei}"/>                                                                                                                                                                                                                       
						</cells>                                                                                                                                                                                                                                                 
					</ColumnListItem>                                                                                                                                                                                                                                         
				</items>                                                                                                                                                                                                                                                   
				<columns>                                                                                                                                                                                                                                                  
					<Column id="sh29" width="80px">                                                                                                                                                                                                                           
						<Label id = "ltt1" text="Order Type"/>                                                                                                                                                                                                                   
						</Column>                                                                                                                                                                                                                                                
				   <Column id="sh30" width="130px">                                                                                                                                                                                                                        
						<Label id="ltt2" text="Description"/>                                                                                                                                                                                                                    
					</Column>                                                                                                                                                                                                                                                 
                                                                                                                                                                                                                                                               
				</columns>                                                                                                                                                                                                                                                 
			</Table>                                                                                                                                                                                                                                                    
					</content>                                                                                                                                                                                                                                                
			<beginButton>                                                                                                                                                                                                                                               
				<Button id="sh31" icon="sap-icon://accept" press="acceptBT" type="Accept" tooltip="Select"/>                                                                                                                                                               
			</beginButton>                                                                                                                                                                                                                                              
			<endButton text="test">                                                                                                                                                                                                                                     
				<Button id="sh32" icon="sap-icon://sys-cancel" press="cancelBT" type="Reject" tooltip="close"/>                                                                                                                                                            
			</endButton>                                                                                                                                                                                                                                                
		</Dialog>                                                                                                                                                                                                                                                    
	</core:FragmentDefinition>                                                                                                                                                                                                                                    