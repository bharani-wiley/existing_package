    IF SY-SUBRC = 0.
    UPDATE ZQTC_I0255_SPLIT .
    ELSE.
    INSERT ZQTC_I0255_SPLIT .
    ENDIF.
    READ TABLE EXTRACT WITH KEY <VIM_XTOTAL_KEY>.
    IF SY-SUBRC EQ 0.
