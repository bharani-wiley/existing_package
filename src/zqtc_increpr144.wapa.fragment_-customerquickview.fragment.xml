<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">                                                                                                                                                                                               
	<QuickView  pages="{ path: 'quickView>/pages', templateShareable: true }" navigate=".onNavigate">                                                                                                                                                             
		<QuickViewPage pageId="{quickView>pageId}" header="{quickView>header}" icon="{quickView>icon}"                                                                                                                                                               
			titleUrl="{quickView>titleUrl}" description="{quickView>description}" groups="{ path: 'quickView>groups', templateShareable: true }">                                                                                                                       
			<QuickViewGroup heading="{quickView>heading}" elements="{ path: 'quickView>elements', templateShareable: true}">                                                                                                                                            
				<QuickViewGroupElement label="{quickView>label}" value="{quickView>value}" url="{quickView>url}" type="{quickView>elementType}"                                                                                                                            
					pageLinkId="{quickView>pageLinkId}" emailSubject="{quickView>emailSubject}" target="{quickView>target}"/>                                                                                                                                                 
			</QuickViewGroup>                                                                                                                                                                                                                                           
		</QuickViewPage>                                                                                                                                                                                                                                             
	</QuickView>                                                                                                                                                                                                                                                  
</core:FragmentDefinition>                                                                                                                                                                                                                                     