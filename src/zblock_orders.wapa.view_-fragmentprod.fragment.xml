<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" controllerName="Block_Orders.Block_Orders.controller.Worklist">                                                                                                                                
	<Dialog resizable="true" draggable="true" contentWidth="530px">                                                                                                                                                                                               
		<content>                                                                                                                                                                                                                                                    
			<Toolbar>                                                                                                                                                                                                                                                   
				<OverflowToolbar>                                                                                                                                                                                                                                          
					<SearchField id="mySearch" search="searchEH" width="500px"/>                                                                                                                                                                                              
				</OverflowToolbar>                                                                                                                                                                                                                                         
				<ToolbarSpacer/>                                                                                                                                                                                                                                           
				<!--<Button icon="sap-icon://clear-filter" press="clearProd" type="" tooltip="Clear Selected Items" width="80px"/>-->                                                                                                                                      
			</Toolbar>                                                                                                                                                                                                                                                  
			<Panel id="mediaPanel" expandable="true" expanded="true" headerText="Selected Items">                                                                                                                                                                       
				<Table items="{sel>/}" id="mySelProdTable" growing="true" mode="Delete" delete="deleteSelectionMedia" showNoData="false">                                                                                                                                  
					<items>                                                                                                                                                                                                                                                   
						<ColumnListItem>                                                                                                                                                                                                                                         
							<cells>                                                                                                                                                                                                                                                 
								<Text text="{sel>Matnr}"/>                                                                                                                                                                                                                             
							</cells>                                                                                                                                                                                                                                                
						</ColumnListItem>                                                                                                                                                                                                                                        
					</items>                                                                                                                                                                                                                                                  
					<columns>                                                                                                                                                                                                                                                 
						<Column width="120px">&nbsp;                                                                                                                                                                                                                             
							<!--<Label text="Media Issue"/>-->                                                                                                                                                                                                                      
						</Column>                                                                                                                                                                                                                                                
					</columns>                                                                                                                                                                                                                                                
				</Table>                                                                                                                                                                                                                                                   
			</Panel>                                                                                                                                                                                                                                                    
			<Table items="{/MaterialSet}" id="myProdTable" growing="true" growingThreshold="100" mode="MultiSelect"                                                                                                                                                     
				selectionChange="rowSelectionMedia">                                                                                                                                                                                                                       
				<items>                                                                                                                                                                                                                                                    
					<ColumnListItem>                                                                                                                                                                                                                                          
						<cells>                                                                                                                                                                                                                                                  
							<Text text="{Matnr}"/>                                                                                                                                                                                                                                  
							<Text text="{Maktx}"/>                                                                                                                                                                                                                                  
						</cells>                                                                                                                                                                                                                                                 
					</ColumnListItem>                                                                                                                                                                                                                                         
				</items>                                                                                                                                                                                                                                                   
				<columns>                                                                                                                                                                                                                                                  
					<Column width="100px">                                                                                                                                                                                                                                    
						<Label text="Material"/>                                                                                                                                                                                                                                 
						</Column>                                                                                                                                                                                                                                                
						<Column width="200px">                                                                                                                                                                                                                                   
							<Label text="Material Description"/>                                                                                                                                                                                                                    
						</Column>                                                                                                                                                                                                                                                
					</columns>                                                                                                                                                                                                                                                
				</Table>                                                                                                                                                                                                                                                   
			</content>                                                                                                                                                                                                                                                  
			<beginButton>                                                                                                                                                                                                                                               
				<Button icon="sap-icon://accept" press="acceptProd" type="Accept" tooltip="Select"/>                                                                                                                                                                       
			</beginButton>                                                                                                                                                                                                                                              
			<endButton text="test">                                                                                                                                                                                                                                     
				<Button icon="sap-icon://sys-cancel" press="cancelProd" type="Reject" tooltip="close"/>                                                                                                                                                                    
			</endButton>                                                                                                                                                                                                                                                
		</Dialog>                                                                                                                                                                                                                                                    
	</core:FragmentDefinition>                                                                                                                                                                                                                                    