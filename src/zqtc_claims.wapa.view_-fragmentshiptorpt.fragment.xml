<core:FragmentDefinition                                                                                                                                                                                                                                       
	xmlns="sap.m"                                                                                                                                                                                                                                                 
	xmlns:core="sap.ui.core">                                                                                                                                                                                                                                     
	<SelectDialog                                                                                                                                                                                                                                                 
		id="valueHelpDialog"                                                                                                                                                                                                                                         
		title="ShipTo ValueHelp"                                                                                                                                                                                                                                     
		items="{/ZSHIPTOSet}"                                                                                                                                                                                                                                        
		search="handleShipToRptSearch"                                                                                                                                                                                                                               
		confirm="handleShipToRptClose"                                                                                                                                                                                                                               
		cancel="handleShipToRptClose"                                                                                                                                                                                                                                
		multiSelect="true">                                                                                                                                                                                                                                          
		<StandardListItem                                                                                                                                                                                                                                            
		    id="shipTolistRpt"                                                                                                                                                                                                                                       
			title="{Shipto}"                                                                                                                                                                                                                                            
			info="{Name2}"                                                                                                                                                                                                                                              
			description="{Name1}"/>                                                                                                                                                                                                                                     
	</SelectDialog>                                                                                                                                                                                                                                               
</core:FragmentDefinition>                                                                                                                                                                                                                                     