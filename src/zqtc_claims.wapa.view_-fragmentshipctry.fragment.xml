<core:FragmentDefinition                                                                                                                                                                                                                                       
	xmlns="sap.m"                                                                                                                                                                                                                                                 
	xmlns:core="sap.ui.core">                                                                                                                                                                                                                                     
	<SelectDialog                                                                                                                                                                                                                                                 
		id="valueHelpDialog"                                                                                                                                                                                                                                         
		title="Country ValueHelp"                                                                                                                                                                                                                                    
		items="{/HT005LandSet}"                                                                                                                                                                                                                                      
		search="handleCtryRptSearch"                                                                                                                                                                                                                                 
		confirm="handleCtryRptClose"                                                                                                                                                                                                                                 
		cancel="handleSCtryRptClose"                                                                                                                                                                                                                                 
		multiSelect="true">                                                                                                                                                                                                                                          
		<StandardListItem                                                                                                                                                                                                                                            
		    id="ctrylistRpt"                                                                                                                                                                                                                                         
			title="{Land1}"                                                                                                                                                                                                                                             
			description="{Landx}"/>                                                                                                                                                                                                                                     
	</SelectDialog>                                                                                                                                                                                                                                               
</core:FragmentDefinition>                                                                                                                                                                                                                                     