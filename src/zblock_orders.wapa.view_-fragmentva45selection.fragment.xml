<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" id="F1" controllerName="Block_Orders.Block_Orders.controller.Worklist"                                                                                                                         
	xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form" xmlns:d="sap.ui.core.LayoutData" xmlns:u="sap.ui.unified" xmlns:c="sap.ui.core">                                                                                                                         
	<Dialog id="dgsel2" resizable="true" draggable="true" contentWidth="650px">                                                                                                                                                                                   
		<content>                                                                                                                                                                                                                                                    
			<l:Splitter orientation="Horizontal">                                                                                                                                                                                                                       
				<f:SimpleForm id="SimpleFormAdd" editable="false" layout="ResponsiveGridLayout">                                                                                                                                                                           
					<f:content>                                                                                                                                                                                                                                               
						<VBox>                                                                                                                                                                                                                                                   
                                                                                                                                                                                                                                                               
							<Label id="I5D1" text="Contract No" width="75px" />                                                                                                                                                                                                     
							<MultiInput id="SOrd45F" showSuggestion="false" showValueHelp="true" valueHelpRequest="onHelpRequestSOrd45EH" width="240px" textAlign="Left"                                                                                                            
								tokenUpdate="SordChange" suggestionItemSelected="onSuggestionBord" placeholder="Contract" visible="true" valueHelpOnly="true"></MultiInput>                                                                                                            
						</VBox>                                                                                                                                                                                                                                                  
						<VBox>                                                                                                                                                                                                                                                   
							<Label id="I5D2" text="Sales Organization * " />                                                                                                                                                                                                        
							<MultiInput id="SOrg45F" showSuggestion="false" showValueHelp="true" valueHelpRequest="onHelpRequestSOrg45EH" width="240px"                                                                                                                             
								tokenUpdate="SorgChange" suggestionItemSelected="onSuggestionBorg" placeholder="Sales Org" visible="true" valueHelpOnly="true"></MultiInput>                                                                                                           
						</VBox>                                                                                                                                                                                                                                                  
						<VBox>                                                                                                                                                                                                                                                   
							<Label id="I5D7" text="Contract Type * "/>                                                                                                                                                                                                              
							<MultiInput showValueHelp="true" id="auart45F" width="240px" valueHelpRequest="onHelpRequestCT45" tokenUpdate="BTChange45"                                                                                                                              
								suggestionItemSelected="onSuggestionCT45" valueHelpOnly="true" showSuggestion="false" placeholder="Order Type"></MultiInput>                                                                                                                           
						</VBox>                                                                                                                                                                                                                                                  
						<VBox>                                                                                                                                                                                                                                                   
							<Label id="I5D8" text="Customer "/>                                                                                                                                                                                                                     
							<MultiInput showValueHelp="true" id="BK45F" width="240px" valueHelpRequest="onHelpRequestBP45" tokenUpdate="BKChange"                                                                                                                                   
								suggestionItemSelected="onSuggestionBK" valueHelpOnly="true" showSuggestion="false" placeholder="Sold-to "></MultiInput>                                                                                                                               
						</VBox>                                                                                                                                                                                                                                                  
						<VBox>                                                                                                                                                                                                                                                   
							<Label id="I5D12" text="Rejection Status "/>                                                                                                                                                                                                            
							<Select id="rej45F" width="240px" change="varDateEH" >                                                                                                                                                                                                  
								<core:Item id="ci1" key="A" text="All Items" enabled="true"/>                                                                                                                                                                                          
								<core:Item id="ci2" key="O" text="Open Items"/>                                                                                                                                                                                                        
								<core:Item id="ci3" key="R" text="Rejected Items"/>                                                                                                                                                                                                    
							</Select>                                                                                                                                                                                                                                               
						</VBox>                                                                                                                                                                                                                                                  
					</f:content>                                                                                                                                                                                                                                              
				</f:SimpleForm>                                                                                                                                                                                                                                            
				<f:SimpleForm id="SimpleFormAdd2" editable="true" layout="ResponsiveGridLayout">                                                                                                                                                                           
					<f:content>                                                                                                                                                                                                                                               
						<VBox>                                                                                                                                                                                                                                                   
							<Label id="I5D3" text="Valid from *" />                                                                                                                                                                                                                 
							<DatePicker id="Bsdate1F" value="" width="240px" valueFormat="yyyy-MM-dd" displayFormat="long" placeholder="Valid from "                                                                                                                                
							change="varDateEH"/>                                                                                                                                                                                                                                    
						</VBox>                                                                                                                                                                                                                                                  
						<VBox>                                                                                                                                                                                                                                                   
							<Label id="I5D4" text="Valid to * " />                                                                                                                                                                                                                  
							<DatePicker id="Bedate1F" value="" width="240px" valueFormat="yyyy-MM-dd" displayFormat="long" placeholder="Valid to "                                                                                                                                  
							change="varDateEH"/>                                                                                                                                                                                                                                    
						</VBox>                                                                                                                                                                                                                                                  
						<VBox>                                                                                                                                                                                                                                                   
							<Label id="I5D5" text="Created on(from) * "/>                                                                                                                                                                                                           
							<DatePicker id="SerdatF" value="" width="240px" valueFormat="yyyy-MM-dd" displayFormat="long" placeholder="Created on"                                                                                                                                  
							change="varDateEH"/>                                                                                                                                                                                                                                    
						</VBox>                                                                                                                                                                                                                                                  
						<VBox>                                                                                                                                                                                                                                                   
							<Label id="I5D6" text="Created on(to) *"/>                                                                                                                                                                                                              
							<DatePicker id="EerdatF" value="" width="240px" valueFormat="yyyy-MM-dd" displayFormat="long" placeholder="Created on"                                                                                                                                  
							change="varDateEH"/>                                                                                                                                                                                                                                    
						</VBox>                                                                                                                                                                                                                                                  
						<VBox>                                                                                                                                                                                                                                                   
							<Label id="I5D9" text="Document date(from) "/>                                                                                                                                                                                                          
							<DatePicker id="SaudatF" value="" width="240px" valueFormat="yyyy-MM-dd" displayFormat="long" placeholder="Document date"                                                                                                                               
							change="varDateEH"/>                                                                                                                                                                                                                                    
						</VBox>                                                                                                                                                                                                                                                  
						<VBox>                                                                                                                                                                                                                                                   
							<Label id="I5D11" text="Document date(to)"/>                                                                                                                                                                                                            
							<DatePicker id="EaudatF" value="" width="240px" valueFormat="yyyy-MM-dd" displayFormat="long" placeholder="Document date"                                                                                                                               
							change="varDateEH"/>                                                                                                                                                                                                                                    
						</VBox>                                                                                                                                                                                                                                                  
					</f:content>                                                                                                                                                                                                                                              
				</f:SimpleForm>                                                                                                                                                                                                                                            
			</l:Splitter>                                                                                                                                                                                                                                               
		</content>                                                                                                                                                                                                                                                   
		<beginButton>                                                                                                                                                                                                                                                
			<Button id="refreshTab3" icon="sap-icon://process" tooltip="Process" press="VA45RefreshEHF" type="Emphasized" visible="true" />                                                                                                                             
		</beginButton>                                                                                                                                                                                                                                               
		<endButton text="close">                                                                                                                                                                                                                                     
			<Button id="gb1" icon="sap-icon://sys-cancel" press="cancelVA45SEL" type="Reject" tooltip="close"/>                                                                                                                                                         
		</endButton>                                                                                                                                                                                                                                                 
	</Dialog>                                                                                                                                                                                                                                                     
</core:FragmentDefinition>                                                                                                                                                                                                                                     