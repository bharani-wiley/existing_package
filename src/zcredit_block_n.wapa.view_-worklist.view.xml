<mvc:View controllerName="cob.v2.ZQTC_CREDIT_BLOCKED_ORDERS_SRV.controller.Worklist" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc"                                                                                                                                 
	xmlns:semantic="sap.f.semantic" xmlns:viz="sap.viz.ui5.controls" xmlns:viz.data="sap.viz.ui5.data" xmlns:core="sap.ui.core"                                                                                                                                   
	xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds" xmlns:fb="sap.ui.comp.filterbar" xmlns:layout="sap.ui.layout">                                                                                                                                            
	<semantic:SemanticPage id="page" headerPinnable="false" toggleHeaderOnTitleClick="false">                                                                                                                                                                     
		<semantic:titleHeading>                                                                                                                                                                                                                                      
			<Title text="{i18n>worklistTitle}" level="H2"/>                                                                                                                                                                                                             
		</semantic:titleHeading>                                                                                                                                                                                                                                     
		<semantic:headerContent>                                                                                                                                                                                                                                     
			<fb:FilterBar id="filterbar" reset="onReset" search="onFilterSearch" filterBarExpanded="true" useToolbar="true" showGoOnFB="true">                                                                                                                          
				<fb:filterItems>                                                                                                                                                                                                                                           
					<fb:FilterItem name="Vkorg" label="Sales Org">                                                                                                                                                                                                            
						<fb:control>                                                                                                                                                                                                                                             
							<Select name="Vkorg" id="cbSalesOrg" ariaDescribedBy="Sales Org" selectedKey="1001" change="onSelectChange" items="{/ZqtcVkorgShSet}">                                                                                                                  
								<core:Item key="{Vkorg}" text="{Vkorg}"/>                                                                                                                                                                                                              
							</Select>                                                                                                                                                                                                                                               
						</fb:control>                                                                                                                                                                                                                                            
					</fb:FilterItem>                                                                                                                                                                                                                                          
					<fb:FilterItem name="Waerk" label="Currency">                                                                                                                                                                                                             
						<fb:control>                                                                                                                                                                                                                                             
							<!--<ComboBox-->                                                                                                                                                                                                                                        
						<Select                                                                                                                                                                                                                                                  
							  name="Waerk" id="cbCurr" ariaLabelledBy="Currency" selectedKey="USD" change="onSelectChange"                                                                                                                                                          
								items="{path: '/FcWaersSet',length:'500'}">                                                                                                                                                                                                            
								<core:Item key="{Waers}" text="{Waers}"/>                                                                                                                                                                                                              
								</Select>                                                                                                                                                                                                                                              
							<!--</ComboBox>-->                                                                                                                                                                                                                                      
						</fb:control>                                                                                                                                                                                                                                            
					</fb:FilterItem>                                                                                                                                                                                                                                          
					<fb:FilterItem name="Agegrp" label="Aging">                                                                                                                                                                                                               
						<fb:control>                                                                                                                                                                                                                                             
							<Select name="Agegrp" id="cbAging" selectedKey="0-30 days" forceSelection="false" change="onSelectChange"  >                                                                                                                                            
								<core:Item key="0-30 days" text="0-30 days"/>                                                                                                                                                                                                          
								<core:Item key="30-60 days" text="30-60 days"/>                                                                                                                                                                                                        
								<core:Item key="30-60 days" text="60-90 days"/>                                                                                                                                                                                                        
								<core:Item key=">90 days" text=">90 days"/>                                                                                                                                                                                                            
								<!--</ComboBox>-->                                                                                                                                                                                                                                     
								                                                                                                                                                                                                                                                       
		                                                                                                                                                                                                                                                             
						</Select>			                                                                                                                                                                                                                                             
							</fb:control>                                                                                                                                                                                                                                           
						</fb:FilterItem>                                                                                                                                                                                                                                         
						<fb:FilterItem name="Auart" label="Document Type">                                                                                                                                                                                                       
							<fb:control>                                                                                                                                                                                                                                            
								<MultiComboBox name="Auart" change="onSelectChange" forceSelection="false" items="{path:'/ShAuartGenSet' ,sorter:{path:'Bezei'},length:'500'}"                                                                                                         
									showSecondaryValues="true">                                                                                                                                                                                                                           
									<core:ListItem key="{Auart}" text="{Bezei}" additionalText="{Auart}"  />                                                                                                                                                                              
								</MultiComboBox>                                                                                                                                                                                                                                       
							</fb:control>                                                                                                                                                                                                                                           
						</fb:FilterItem>                                                                                                                                                                                                                                         
					</fb:filterItems>                                                                                                                                                                                                                                         
				</fb:FilterBar>                                                                                                                                                                                                                                            
			</semantic:headerContent>                                                                                                                                                                                                                                   
			<semantic:content>                                                                                                                                                                                                                                          
				<viz:Popover id="idPopOver"></viz:Popover>                                                                                                                                                                                                                 
				<viz:VizFrame width="100%" height='100%' id="idVizFrame" uiConfig="{applicationSet:'fiori'}" vizType='stacked_bar' selectData="onSelectData"                                                                                                               
					vizProperties="{dataLabel:{ showTotal: false},title:{ text:''}}">                                                                                                                                                                                         
					<viz:dataset>                                                                                                                                                                                                                                             
						<viz.data:FlattenedDataset id="idVizDataSet">                                                                                                                                                                                                            
							<viz.data:dimensions>                                                                                                                                                                                                                                   
								<viz.data:DimensionDefinition name="DOCUMENT TYPE" value="{Bezei}"/>                                                                                                                                                                                   
								<!--<viz.data:DimensionDefinition name="Amount1" value="{Amount1}"/>-->                                                                                                                                                                                
								<!--<viz.data:DimensionDefinition name="Amount2" value="{Amount2}"/>-->                                                                                                                                                                                
								<!--<viz.data:DimensionDefinition name="Amount3" value="{Amount3}"/>-->                                                                                                                                                                                
								<!--<viz.data:DimensionDefinition name="Amount4" value="{Amount4}"/>-->                                                                                                                                                                                
								<!--<viz.data:DimensionDefinition name="COUNT" value="{graphData>count}"/>-->                                                                                                                                                                          
							</viz.data:dimensions>                                                                                                                                                                                                                                  
							<viz.data:measures>                                                                                                                                                                                                                                     
								<viz.data:MeasureDefinition name="0-30K" value="{Count1}">                                                                                                                                                                                             
								</viz.data:MeasureDefinition>                                                                                                                                                                                                                          
								<viz.data:MeasureDefinition name="31K-60K" value="{Count2}"/>                                                                                                                                                                                          
								<viz.data:MeasureDefinition name="61K-90K" value="{Count3}"/>                                                                                                                                                                                          
								<viz.data:MeasureDefinition name=">90K" value="{Count4}"/>                                                                                                                                                                                             
								</viz.data:measures>                                                                                                                                                                                                                                   
							</viz.data:FlattenedDataset>                                                                                                                                                                                                                            
						</viz:dataset>                                                                                                                                                                                                                                           
						<viz:feeds>                                                                                                                                                                                                                                              
							<viz.feeds:FeedItem id='valueAxisFeed' uid="valueAxis" type="Measure" values="0-30K,31K-60K,61K-90K,>90K"/>                                                                                                                                             
							<viz.feeds:FeedItem uid="categoryAxis" type="Dimension" values="DOCUMENT TYPE"/>                                                                                                                                                                        
						</viz:feeds>                                                                                                                                                                                                                                             
					</viz:VizFrame>                                                                                                                                                                                                                                           
				</semantic:content>                                                                                                                                                                                                                                        
			</semantic:SemanticPage>                                                                                                                                                                                                                                    
		</mvc:View>                                                                                                                                                                                                                                                  