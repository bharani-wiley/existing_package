   WHEN GELOESCHT.
  SELECT SINGLE FOR UPDATE * FROM ZQTC_RP_DETER WHERE
  KDGRP = ZQTCV_RP_DETER-KDGRP AND
  KONDA = ZQTCV_RP_DETER-KONDA AND
  PAY_TYPE = ZQTCV_RP_DETER-PAY_TYPE AND
