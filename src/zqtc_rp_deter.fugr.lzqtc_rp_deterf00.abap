  KDKG2 = ZQTCV_RP_DETER-KDKG2 AND
  MVGR5 = ZQTCV_RP_DETER-MVGR5 AND
  BSARK = ZQTCV_RP_DETER-BSARK AND
  VKORG = ZQTCV_RP_DETER-VKORG AND
  WERKS = ZQTCV_RP_DETER-WERKS AND
  MATNR = ZQTCV_RP_DETER-MATNR AND
  ZZACTION = ZQTCV_RP_DETER-ZZACTION .
    IF SY-SUBRC = 0.
    DELETE ZQTC_RP_DETER .
    ENDIF.
    IF STATUS-DELETE EQ GELOESCHT.
      READ TABLE EXTRACT WITH KEY <VIM_XTOTAL_KEY> BINARY SEARCH.
      DELETE EXTRACT INDEX SY-TABIX.
    ENDIF.
    DELETE TOTAL.
    IF X_HEADER-DELMDTFLAG NE SPACE.
      PERFORM DELETE_FROM_MAINKEY_TAB.
    ENDIF.
   WHEN OTHERS.
