<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc">                                                                                                                                                                   
	<Dialog resizable="true" title="Delivery Block Summary">                                                                                                                                                                                                      
	                                                                                                                                                                                                                                                              
				<Table id="dlgListMessageModel" items="{msg>/}" fixedLayout="false"&nbsp;                                                                                                                                                                                  
								growing="true" growingThreshold="6" mode="None" showSeparators="Inner" ariaLabelledBy="title"                                                                                                                                                          
								backgroundDesign="Solid" includeItemInSelection="true">                                                                                                                                                                                                
								<columns >                                                                                                                                                                                                                                             
									<Column>                                                                                                                                                                                                                                              
										<Label text="Document No" textDirection="Inherit" design="Bold"/>                                                                                                                                                                                    
									</Column>                                                                                                                                                                                                                                             
									<Column>                                                                                                                                                                                                                                              
										<Label text="Message Type" textDirection="Inherit" design="Bold"/>                                                                                                                                                                                   
									</Column>                                                                                                                                                                                                                                             
									<Column>                                                                                                                                                                                                                                              
										<Label text="Message Description" textDirection="Inherit" design="Bold"/>                                                                                                                                                                            
									</Column>                                                                                                                                                                                                                                             
								                                                                                                                                                                                                                                                       
								</columns>                                                                                                                                                                                                                                             
								<items>                                                                                                                                                                                                                                                
									<ColumnListItem >                                                                                                                                                                                                                                     
										<cells>                                                                                                                                                                                                                                              
											<Text text="{msg>Vbeln}" width="100px"/>                                                                                                                                                                                                            
											<Text text="{msg>Type}" width="100px"/>                                                                                                                                                                                                             
											<Text text="{msg>Msg1} {msg>Msg2} {msg>Msg3}"/>                                                                                                                                                                                                     
										                                                                                                                                                                                                                                                     
										</cells>                                                                                                                                                                                                                                             
									</ColumnListItem>                                                                                                                                                                                                                                     
								</items>                                                                                                                                                                                                                                               
							</Table>                                                                                                                                                                                                                                                
		                                                                                                                                                                                                                                                             
		<beginButton>                                                                                                                                                                                                                                                
			<Button press="onMessageDlgClose" text="Close"></Button>                                                                                                                                                                                                    
		</beginButton>                                                                                                                                                                                                                                               
		<endButton>                                                                                                                                                                                                                                                  
			<Button press="downlaodRelDocs" text="Export"></Button>                                                                                                                                                                                                     
		</endButton>                                                                                                                                                                                                                                                 
	</Dialog>                                                                                                                                                                                                                                                     
	<!--<MessagePopover items="{mModel>/}" initiallyExpanded="true" >-->                                                                                                                                                                                          
	<!--	<MessagePopoverItem title="{mModel>Vbeln}" type="Success"/>-->                                                                                                                                                                                           
	<!--</MessagePopover>-->                                                                                                                                                                                                                                      
</core:FragmentDefinition>                                                                                                                                                                                                                                     