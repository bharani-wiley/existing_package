   <cls:CL_FP_CODING id="o5">
    <CL_FP_INTERFACE_DATA_CHANGED classVersion="1">
     <INTERFACE_DATA href="#o1"/>
    </CL_FP_INTERFACE_DATA_CHANGED>
    <CL_FP_CODING classVersion="1">
     <INPUT_PARAMETERS>
      <FPPARAMETER>IM_HDR</FPPARAMETER>
     </INPUT_PARAMETERS>
     <OUTPUT_PARAMETERS>
      <FPPARAMETER>V_TEXT</FPPARAMETER>
     </OUTPUT_PARAMETERS>
     <INITIALIZATION>
      <FPCLINE>* DATA:  li_lines     TYPE STANDARD TABLE OF tline. &quot;Lines of text read</FPCLINE>
      <FPCLINE>*</FPCLINE>
      <FPCLINE>* CALL FUNCTION &apos;READ_TEXT&apos;</FPCLINE>
      <FPCLINE>*   EXPORTING</FPCLINE>
      <FPCLINE>*     id                      = &apos;ST&apos;</FPCLINE>
      <FPCLINE>*     language                = sy-langu</FPCLINE>
      <FPCLINE>*     name                    = im_hdr-trans_name</FPCLINE>
      <FPCLINE>*     object                  = &apos;TEXT&apos;</FPCLINE>
      <FPCLINE>*   TABLES</FPCLINE>
      <FPCLINE>*     lines                   = li_lines</FPCLINE>
      <FPCLINE>*   EXCEPTIONS</FPCLINE>
      <FPCLINE>*     id                      = 1</FPCLINE>
      <FPCLINE>*     language                = 2</FPCLINE>
      <FPCLINE>*     name                    = 3</FPCLINE>
      <FPCLINE>*     not_found               = 4</FPCLINE>
      <FPCLINE>*     object                  = 5</FPCLINE>
      <FPCLINE>*     reference_check         = 6</FPCLINE>
      <FPCLINE>*     wrong_access_to_archive = 7</FPCLINE>
      <FPCLINE>*     OTHERS                  = 8.</FPCLINE>
      <FPCLINE>* IF sy-subrc EQ 0.</FPCLINE>
      <FPCLINE>*   LOOP AT li_lines ASSIGNING FIELD-SYMBOL(&lt;lw_lines&gt;).</FPCLINE>
      <FPCLINE>*     REPLACE ALL OCCURRENCES OF &apos;&amp;st_name&amp;&apos; IN &lt;lw_lines&gt;-tdline</FPCLINE>
      <FPCLINE>*     WITH im_hdr-st_name.</FPCLINE>
      <FPCLINE>*     REPLACE ALL OCCURRENCES OF &apos;&amp;PROD_CODE&amp;&apos; IN &lt;lw_lines&gt;-tdline</FPCLINE>
      <FPCLINE>*     WITH im_hdr-prod_code.</FPCLINE>
      <FPCLINE>*     REPLACE ALL OCCURRENCES OF &apos;&amp;COURSE_TIT&amp;&apos; IN &lt;lw_lines&gt;-tdline</FPCLINE>
      <FPCLINE>*     WITH im_hdr-sales_text.</FPCLINE>
      <FPCLINE>*     REPLACE ALL OCCURRENCES OF &apos;&amp;GRADE_DATE&amp;&apos; IN &lt;lw_lines&gt;-tdline</FPCLINE>
      <FPCLINE>*     WITH im_hdr-grade_date.</FPCLINE>
      <FPCLINE>*     REPLACE ALL OCCURRENCES OF &apos;&amp;GRADE&amp;&apos; IN &lt;lw_lines&gt;-tdline</FPCLINE>
      <FPCLINE>*     WITH im_hdr-grade.</FPCLINE>
      <FPCLINE>*     REPLACE ALL OCCURRENCES OF &apos;&amp;CREDIT_DIS&amp;&apos; IN &lt;lw_lines&gt;-tdline</FPCLINE>
      <FPCLINE>*     WITH im_hdr-credit_dis.</FPCLINE>
      <FPCLINE>*     REPLACE ALL OCCURRENCES OF &apos;&amp;UNIV_NAME&amp;&apos; IN &lt;lw_lines&gt;-tdline</FPCLINE>
      <FPCLINE>*      WITH im_hdr-univ_name.</FPCLINE>
      <FPCLINE>*        CONCATENATE v_text &lt;lw_lines&gt;-tdline into v_text.</FPCLINE>
      <FPCLINE>*   ENDLOOP.</FPCLINE>
      <FPCLINE>* ENDIF. &quot; IF sy-subrc EQ 0</FPCLINE>
      <FPCLINE>* CONCATENATE &apos;&lt;body xmlns=&quot;http://www.w3.org/1999/xhtml&quot; xmlns:xfa=&quot;http://www.xfa.org/schema/xfa-data/1.0/&quot;&gt;&apos;</FPCLINE>
      <FPCLINE>* v_text &apos;&lt;/body&gt;&apos; INTO v_text.</FPCLINE>
     </INITIALIZATION>
     <FORMS/>
    </CL_FP_CODING>
