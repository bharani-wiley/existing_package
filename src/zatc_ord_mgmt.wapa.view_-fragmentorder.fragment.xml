<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" controllerName="Block_Orders.Block_Orders.controller.Worklist">                                                                                                                                
	<Dialog id="sh8" resizable="true" draggable="true" contentWidth="450px">                                                                                                                                                                                      
		<content>                                                                                                                                                                                                                                                    
			<Toolbar id="sh7">                                                                                                                                                                                                                                          
				<OverflowToolbar id="sh6">                                                                                                                                                                                                                                 
					<SearchField id="mySearcho" search="searchEHO" width="270px"/>                                                                                                                                                                                            
				</OverflowToolbar>                                                                                                                                                                                                                                         
				<ToolbarSpacer id="sh4"/>                                                                                                                                                                                                                                  
				<Button id="sh5" icon="sap-icon://clear-filter" press="clearBSO" type="Accept" tooltip="Clear Selected Orders" width="80px"/>                                                                                                                              
			</Toolbar>                                                                                                                                                                                                                                                  
				<Panel id="BsoPanel" expandable="true" expanded="true" headerText="Selected Items">                                                                                                                                                                        
				<Table items="{Bsosel>/}" id="BSOselTable" growing="true" mode="Delete"                                                                                                                                                                                    
				delete="deleteSelectionBso" showNoData="false" >                                                                                                                                                                                                           
					<items>                                                                                                                                                                                                                                                   
						<ColumnListItem id="sh9">                                                                                                                                                                                                                                
							<cells>                                                                                                                                                                                                                                                 
								<Text id="sh10" text="{Bsosel>Vbeln}"/>                                                                                                                                                                                                                
							</cells>                                                                                                                                                                                                                                                
						</ColumnListItem>                                                                                                                                                                                                                                        
						</items>                                                                                                                                                                                                                                                 
						<columns>                                                                                                                                                                                                                                                
							<Column id="sh11" width="150px">                                                                                                                                                                                                                        
								<!--<Label id="sd1" text=" Select All/Deselect All"/>-->                                                                                                                                                                                               
							</Column>                                                                                                                                                                                                                                               
						</columns>                                                                                                                                                                                                                                               
					</Table>                                                                                                                                                                                                                                                  
				</Panel>                                                                                                                                                                                                                                                   
				                                                                                                                                                                                                                                                           
<!--				rows="{Billing>/results}"-->                                                                                                                                                                                                                           
<!--text="{Billing>Auart}"-->                                                                                                                                                                                                                                  
                                                                                                                                                                                                                                                               
			<Table items="{Billing>/results}" id="BSOTable" growing="true" growingThreshold="75" mode="MultiSelect"                                                                                                                                                     
			       	selectionChange="rowSelectionBso" >                                                                                                                                                                                                                 
				<items>                                                                                                                                                                                                                                                    
					<ColumnListItem id="sh12">                                                                                                                                                                                                                                
						<cells>                                                                                                                                                                                                                                                  
							<Text id="sh13" text="{Billing>Vbeln}"/>                                                                                                                                                                                                                
						</cells>                                                                                                                                                                                                                                                 
					</ColumnListItem>                                                                                                                                                                                                                                         
				</items>                                                                                                                                                                                                                                                   
				<columns>                                                                                                                                                                                                                                                  
					<Column id="sh14" width="75px">                                                                                                                                                                                                                           
						<Label id="sh15" 	 text=" Select All/ Deselect All"/>                                                                                                                                                                                                    
					</Column>                                                                                                                                                                                                                                                 
                                                                                                                                                                                                                                                               
				</columns>                                                                                                                                                                                                                                                 
			</Table>                                                                                                                                                                                                                                                    
					</content>                                                                                                                                                                                                                                                
			<beginButton>                                                                                                                                                                                                                                               
				<Button id="sh17" icon="sap-icon://accept" press="acceptBSO" type="Accept" tooltip="Select"/>                                                                                                                                                              
			</beginButton>                                                                                                                                                                                                                                              
			<endButton text="test">                                                                                                                                                                                                                                     
				<Button id="sh19" icon="sap-icon://sys-cancel" press="cancelBSO" type="Reject" tooltip="close"/>                                                                                                                                                           
			</endButton>                                                                                                                                                                                                                                                
		</Dialog>                                                                                                                                                                                                                                                    
	</core:FragmentDefinition>                                                                                                                                                                                                                                    