<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" controllerName="Block_Orders.Block_Orders.controller.Worklist">                                                                                                                                
	<Dialog resizable="true" draggable="true" contentWidth="530px">                                                                                                                                                                                               
		<content>                                                                                                                                                                                                                                                    
			<Toolbar>                                                                                                                                                                                                                                                   
				<OverflowToolbar>                                                                                                                                                                                                                                          
					<SearchField id="mySearch1" search="searchEH2" width="500px"/>                                                                                                                                                                                            
				</OverflowToolbar>                                                                                                                                                                                                                                         
				<ToolbarSpacer/>                                                                                                                                                                                                                                           
				<Button icon="sap-icon://clear-filter" press="clearcustomer" type="" tooltip="Clear Selected Items" width="80px"/>                                                                                                                                         
			</Toolbar>                                                                                                                                                                                                                                                  
			<Panel id="mediaPanel1" expandable="true" expanded="true" headerText="Selected Customers">                                                                                                                                                                  
				<Table items="{sel1>/}" id="mySelProdTable1" growing="true" mode="Delete" delete="deleteSelectioncustomer" showNoData="false">                                                                                                                             
					<items>                                                                                                                                                                                                                                                   
						<ColumnListItem>                                                                                                                                                                                                                                         
							<cells>                                                                                                                                                                                                                                                 
								<Text text="{sel1>Kunnr}"/>                                                                                                                                                                                                                            
							</cells>                                                                                                                                                                                                                                                
						</ColumnListItem>                                                                                                                                                                                                                                        
					</items>                                                                                                                                                                                                                                                  
					<columns>                                                                                                                                                                                                                                                 
						<Column width="120px">                                                                                                                                                                                                                                   
							<!--<Label text="Media Issue"/>-->                                                                                                                                                                                                                      
						</Column>                                                                                                                                                                                                                                                
					</columns>                                                                                                                                                                                                                                                
				</Table>                                                                                                                                                                                                                                                   
			</Panel>                                                                                                                                                                                                                                                    
			<Table items="{/SoldtoSet}" id="myProdTable1" growing="true" growingThreshold="100" mode="MultiSelect"                                                                                                                                                      
				selectionChange="rowSelectioncustomer">                                                                                                                                                                                                                    
				<items>                                                                                                                                                                                                                                                    
					<ColumnListItem>                                                                                                                                                                                                                                          
						<cells>                                                                                                                                                                                                                                                  
							<Text text="{Kunnr}"/>                                                                                                                                                                                                                                  
							<Text text="{Name1}{Name2}"/>                                                                                                                                                                                                                           
						</cells>                                                                                                                                                                                                                                                 
					</ColumnListItem>                                                                                                                                                                                                                                         
				</items>                                                                                                                                                                                                                                                   
				<columns>                                                                                                                                                                                                                                                  
					<Column width="100px">                                                                                                                                                                                                                                    
						<Label text="Customer No"/>                                                                                                                                                                                                                              
						</Column>                                                                                                                                                                                                                                                
						<Column width="200px">                                                                                                                                                                                                                                   
							<Label text="Name"/>                                                                                                                                                                                                                                    
						</Column>                                                                                                                                                                                                                                                
					</columns>                                                                                                                                                                                                                                                
				</Table>                                                                                                                                                                                                                                                   
			</content>                                                                                                                                                                                                                                                  
			<beginButton>                                                                                                                                                                                                                                               
				<Button icon="sap-icon://accept" press="acceptcustomer" type="Accept" tooltip="Select"/>                                                                                                                                                                   
			</beginButton>                                                                                                                                                                                                                                              
			<endButton text="test">                                                                                                                                                                                                                                     
				<Button icon="sap-icon://sys-cancel" press="cancelcustomer" type="Reject" tooltip="close"/>                                                                                                                                                                
			</endButton>                                                                                                                                                                                                                                                
		</Dialog>                                                                                                                                                                                                                                                    
	</core:FragmentDefinition>                                                                                                                                                                                                                                    