<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="recon.ZRECON_DASHBOARD.controller.target"                                                                                                                         
	xmlns:html="http://www.w3.org/1999/xhtml">                                                                                                                                                                                                                    
	<App>                                                                                                                                                                                                                                                         
		<pages>                                                                                                                                                                                                                                                      
			<Page showNavButton="false" navButtonPress="onPageBack">                                                                                                                                                                                                    
				<headerContent>                                                                                                                                                                                                                                            
					<HBox>                                                                                                                                                                                                                                                    
						<Button icon="sap-icon://nav-back" tooltip="Back" press="onPageBack" class="styleclass2"/>                                                                                                                                                               
						<Image src="Logo/Wileylogo.GIF" height="50px"/>                                                                                                                                                                                                          
						<Text text="Reconciliation Dashboard" textAlign="Center" width="1100px" class="styleclass1"/>                                                                                                                                                            
						<!--<Text text=". " textAlign="End" width="200px" class="styleclass1"/>-->                                                                                                                                                                               
					</HBox>                                                                                                                                                                                                                                                   
				</headerContent>                                                                                                                                                                                                                                           
				<content>                                                                                                                                                                                                                                                  
					<ObjectHeader responsive="true" fullScreenOptimized="true" titleActive="true" title="{Devid} {Description}" backgroundDesign="Translucent"                                                                                                                
						id="objHead" condensed="true" numberUnit="{Devid}" titleLevel="H4">                                                                                                                                                                                      
						<!--<ObjectAttribute title="Date Range" text="" id="headerDate"/>-->                                                                                                                                                                                     
						<!--<DatePicker id="IDatefrom" valueFormat="yyyy-MM-dd" change="handleChange" displayFormat="long"/>-->                                                                                                                                                  
						<statuses>                                                                                                                                                                                                                                               
							<ObjectStatus title="Target" text="{Target}" state="Success"/>                                                                                                                                                                                          
						</statuses>                                                                                                                                                                                                                                              
					</ObjectHeader>                                                                                                                                                                                                                                           
					<Table inset="false" id="targetTab" items="{idoc>/results}" growing="true" growingThreshold="8">                                                                                                                                                          
						<headerToolbar>                                                                                                                                                                                                                                          
							<OverflowToolbar design="Transparent" height="2rem">                                                                                                                                                                                                    
								<content>                                                                                                                                                                                                                                              
									<!--<DateRangeSelection width="20%"></DateRangeSelection>-->                                                                                                                                                                                          
									<SearchField width="30%" class="sapUiSmallMargin" search="onTabSearch"/>                                                                                                                                                                              
									<Label text="Date Range" labelFor="DatePicker"/>                                                                                                                                                                                                      
									<DatePicker id="IDatefrom2" valueFormat="yyyy-MM-dd" change="handleChange" displayFormat="short" width="9%" tooltip="FromDate"/>                                                                                                                      
									<DatePicker id="IDateto2" valueFormat="yyyy-MM-dd" change="handleChange" width="9%" displayFormat="short" tooltip="ToDate"/>                                                                                                                          
									<!--<ToolbarSpacer/>-->                                                                                                                                                                                                                               
									<Button icon="sap-icon://refresh" type="Emphasized" tooltip="Refresh Data" press="refreshTarget"/>                                                                                                                                                    
									<ToolbarSpacer/>                                                                                                                                                                                                                                      
									<Button id="sort" tooltip="Sort" icon="sap-icon://sort" press="handleSortButtonPressed" type="Emphasized" />                                                                                                                                          
									<Button id = "filter" tooltip="Filter" icon="sap-icon://filter" press="handleFilterButtonPressed" type="Emphasized"/>                                                                                                                                 
									<Button icon="sap-icon://excel-attachment" tooltip="Download Excel" type="Emphasized" press="downloadIdocs"/>                                                                                                                                         
								</content>                                                                                                                                                                                                                                             
							</OverflowToolbar>                                                                                                                                                                                                                                      
						</headerToolbar>                                                                                                                                                                                                                                         
						<columns>                                                                                                                                                                                                                                                
							<Column width="12em">                                                                                                                                                                                                                                   
								<Text text="Doc. Number"/>                                                                                                                                                                                                                             
							</Column>                                                                                                                                                                                                                                               
							<Column width="8em">                                                                                                                                                                                                                                    
								<Text text="Created Date"/>                                                                                                                                                                                                                            
							</Column>                                                                                                                                                                                                                                               
							<Column width="8em">                                                                                                                                                                                                                                    
								<Text text="Log Date"/>                                                                                                                                                                                                                                
							</Column>                                                                                                                                                                                                                                               
							<!--<Column>-->                                                                                                                                                                                                                                         
							<!--	<Text text="Message Type"/>-->                                                                                                                                                                                                                     
							<!--</Column>-->                                                                                                                                                                                                                                        
							<Column>                                                                                                                                                                                                                                                
								<Text text="Error Message"/>                                                                                                                                                                                                                           
							</Column>                                                                                                                                                                                                                                               
						</columns>                                                                                                                                                                                                                                               
						<items>                                                                                                                                                                                                                                                  
							<ColumnListItem>                                                                                                                                                                                                                                        
								<cells>                                                                                                                                                                                                                                                
									<Link text="{idoc>Docnum}" href="{idoc>Link}" target="_blank"/>                                                                                                                                                                                       
									<!--<ObjectStatus text="{Docnum}" state="Information" class="sapUiTinyMargin" inverted="true"/>-->                                                                                                                                                    
									<Text text="{idoc>Credat}"></Text>                                                                                                                                                                                                                    
									<Text text="{idoc>Logdat}"></Text>                                                                                                                                                                                                                    
									<!--<Text text="{Statyp}"></Text>-->                                                                                                                                                                                                                  
									<ObjectStatus text="{idoc>Message}" state="Error"/>                                                                                                                                                                                                   
								</cells>                                                                                                                                                                                                                                               
							</ColumnListItem>                                                                                                                                                                                                                                       
						</items>                                                                                                                                                                                                                                                 
					</Table>                                                                                                                                                                                                                                                  
				</content>                                                                                                                                                                                                                                                 
			</Page>                                                                                                                                                                                                                                                     
		</pages>                                                                                                                                                                                                                                                     
	</App>                                                                                                                                                                                                                                                        
</mvc:View>                                                                                                                                                                                                                                                    