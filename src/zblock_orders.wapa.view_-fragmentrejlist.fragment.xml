<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" controllerName="Block_Orders.Block_Orders.controller.Worklist">                                                                                                                                
                                                                                                                                                                                                                                                               
	<Dialog id="dg1" resizable="true" draggable="true" contentWidth="1100px">                                                                                                                                                                                     
		<content>                                                                                                                                                                                                                                                    
			<Table items="{RejItems>/results}" id="TableRL" growing="true" growingThreshold="100"&nbsp;                                                                                                                                                                 
			       	selectionChange="rowSelectionord" >                                                                                                                                                                                                                 
				<items>                                                                                                                                                                                                                                                    
					<ColumnListItem id="cls1">                                                                                                                                                                                                                                
                                                                                                                                                                                                                                                               
					<cells>                                                                                                                                                                                                                                                   
							<Link text="{RejItems>VBELN}" href="{RejItems>LINK}" target="_blank"/>                                                                                                                                                                                  
							<Text text="{RejItems>POSNR}"/>                                                                                                                                                                                                                         
							<Text text="{RejItems>MATNR}"/>                                                                                                                                                                                                                         
							<Text text="{RejItems>VBEGDAT}"/>                                                                                                                                                                                                                       
							<Text text="{RejItems>VENDDAT}"/>                                                                                                                                                                                                                       
							<Text text="{RejItems>ABGRU} - {RejItems>ABGRUT}"/>                                                                                                                                                                                                     
							<!--<Text text="{RejItems>ABGRUT}"/>-->                                                                                                                                                                                                                 
							<Text text="{RejItems>NETWR}"/>                                                                                                                                                                                                                         
							<Text text="{RejItems>WAERK}"/>                                                                                                                                                                                                                         
							<Text text="{RejItems>KWMENG}"/>                                                                                                                                                                                                                        
							<Text text="{RejItems>VRKME}"/>                                                                                                                                                                                                                         
							<Text text="{RejItems>BSTKD}"/>                                                                                                                                                                                                                         
							<Text text="{RejItems>KDGRP}"/>                                                                                                                                                                                                                         
						</cells>		                                                                                                                                                                                                                                               
					</ColumnListItem>                                                                                                                                                                                                                                         
				</items>                                                                                                                                                                                                                                                   
				<columns>                                                                                                                                                                                                                                                  
					<Column width="100px">                                                                                                                                                                                                                                    
						<Label text="Sales Order"/>                                                                                                                                                                                                                              
					</Column>                                                                                                                                                                                                                                                 
					<Column width="100px">                                                                                                                                                                                                                                    
						<Label text="Item"/>                                                                                                                                                                                                                                     
					</Column>                                                                                                                                                                                                                                                 
					<Column width="100px">                                                                                                                                                                                                                                    
						<Label text="Material"/>                                                                                                                                                                                                                                 
					</Column>                                                                                                                                                                                                                                                 
					<Column width="100px">                                                                                                                                                                                                                                    
						<Label text="Contract Start date"/>                                                                                                                                                                                                                      
					</Column>                                                                                                                                                                                                                                                 
					<Column width="100px">                                                                                                                                                                                                                                    
						<Label text="Contract End date"/>                                                                                                                                                                                                                        
					</Column>                                                                                                                                                                                                                                                 
					<Column width="180px">                                                                                                                                                                                                                                    
						<Label text="Rejection Status"/>                                                                                                                                                                                                                         
					</Column>                                                                                                                                                                                                                                                 
					<Column width="100px">                                                                                                                                                                                                                                    
						<Label text="Item Price"/>                                                                                                                                                                                                                               
					</Column>                                                                                                                                                                                                                                                 
					<Column width="100px">                                                                                                                                                                                                                                    
						<Label text="Currency"/>                                                                                                                                                                                                                                 
					</Column>                                                                                                                                                                                                                                                 
					<Column width="100px">                                                                                                                                                                                                                                    
						<Label text="Quantity"/>                                                                                                                                                                                                                                 
					</Column>                                                                                                                                                                                                                                                 
					<Column width="100px">                                                                                                                                                                                                                                    
						<Label text="UoM"/>                                                                                                                                                                                                                                      
					</Column>                                                                                                                                                                                                                                                 
					<Column width="100px">                                                                                                                                                                                                                                    
						<Label text="PO Number"/>                                                                                                                                                                                                                                
					</Column>                                                                                                                                                                                                                                                 
					<Column width="120px">                                                                                                                                                                                                                                    
						<Label text="Customer Group"/>                                                                                                                                                                                                                           
					</Column>                                                                                                                                                                                                                                                 
				</columns>                                                                                                                                                                                                                                                 
			</Table>                                                                                                                                                                                                                                                    
                                                                                                                                                                                                                                                               
			</content>                                                                                                                                                                                                                                                  
			<endButton text="test">                                                                                                                                                                                                                                     
				<Button icon="sap-icon://sys-cancel" press="cancelREJItem" type="Reject" tooltip="close"/>                                                                                                                                                                 
			</endButton>                                                                                                                                                                                                                                                
		</Dialog>                                                                                                                                                                                                                                                    
	</core:FragmentDefinition>                                                                                                                                                                                                                                    